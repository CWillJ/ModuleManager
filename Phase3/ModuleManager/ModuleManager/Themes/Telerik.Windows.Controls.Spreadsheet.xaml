        <Setter Property="Width" Value="534"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:BorderTabContent">
                    <Grid Background="{TemplateBinding Background}" Margin="4 2 5 5" VerticalAlignment="Top">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" MinWidth="150"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <dialogs:BorderStyleSelector x:Name="TableBorderSelector" CurrentBorder="{Binding Path=CurrentBorder, Mode=TwoWay}" Margin="5 0 0 0" ColorPalette="{Binding Path=ColorPalette}"/>
                        <dialogs:BorderSelector x:Name="BorderSelector"
                                Grid.Column="1"
                                UseLayoutRounding="True"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Stretch"
                                LeftBorder="{Binding Path=LeftBorder, Mode=TwoWay}"
                                TopBorder="{Binding Path=TopBorder, Mode=TwoWay}"
                                RightBorder="{Binding Path=RightBorder, Mode=TwoWay}"
                                BottomBorder="{Binding Path=BottomBorder, Mode=TwoWay}"
                                InsideHorizontalBorder="{Binding Path=InsideHorizontalBorder, Mode=TwoWay}"
                                InsideVerticalBorder="{Binding Path=InsideVerticalBorder, Mode=TwoWay}"
                                DiagonalDownBorder="{Binding Path=DiagonalDownBorder, Mode=TwoWay}"
                                DiagonalUpBorder="{Binding Path=DiagonalUpBorder, Mode=TwoWay}"
                                SelectedColumnsCount="{Binding Path=SelectedColumnsCount}"
                                SelectedRowsCount="{Binding Path=SelectedRowsCount}"
                                CurrentBorder="{Binding Path=CurrentBorder}"
                                ColorPalette="{Binding Path=ColorPalette}"
                                Margin="10 0 0 0"/>
                        <TextBlock Grid.Row="1" Grid.ColumnSpan="2" TextWrapping="Wrap" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Border_Info" Margin="0 10 0 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FillTabContentStyle" TargetType="dialogs:FillTabContent">
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Width" Value="516"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:FillTabContent">
                    <Grid Margin="0 6 10 5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition Height="75"/>
                        </Grid.RowDefinitions>
                        <Border Background="{TemplateBinding Background}" Grid.ColumnSpan="2" Grid.RowSpan="2"/>
                        <StackPanel HorizontalAlignment="Left">
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_BackgroundColor" Margin="0 0 0 5"/>
                            <telerik:RadToggleButton x:Name="ButtonNoColor" Margin="0 0 0 4" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_NoColor"/>
                            <telerik:RadColorSelector x:Name="BackgroundColorSelector"
                                    AutomaticColor="White"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    SelectedColor="{Binding Path=BackgroundColor, Mode=TwoWay}"
                                    HeaderPaletteItemsSource="{Binding Path=ColorPalette.HeaderPaletteItemsSource}"
                                    MainPaletteItemsSource="{Binding Path=ColorPalette.MainPaletteItemsSource}"
                                    MainPaletteOrientation="{Binding Path=ColorPalette.MainPaletteOrientation}"
                                    Style="{StaticResource RadColorSelectorStylePredefined}"/>
                            <Grid Margin="0 10 0 0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <telerik:RadButton x:Name="FillEffectsButton" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_FillEffects" VerticalAlignment="Center" Width="100"/>
                                <telerik:RadButton x:Name="MoreColorsButton"
                                        Grid.Column="2"
                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_MoreColors"
                                        HorizontalAlignment="Right"
                                        Margin="5 0 0 0"
                                        VerticalAlignment="Center"
                                        Width="100"/>
                            </Grid>
                        </StackPanel>
                        <StackPanel Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" Margin="20 0 0 0">
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_PatternColor" Margin="0 0 0 5"/>
                            <spreadsheetControls:ColorSelectorDropDown x:Name="PatternsColorSelector"
                                    AutomaticColor="Black"
                                    SelectedColor="{Binding Path=PatternColor, Mode=TwoWay}"
                                    HeaderPaletteItemsSource="{Binding Path=ColorPalette.HeaderPaletteItemsSource}"
                                    MainPaletteItemsSource="{Binding Path=ColorPalette.MainPaletteItemsSource}"
                                    MainPaletteOrientation="{Binding Path=ColorPalette.MainPaletteOrientation}"
                                    Width="153"
                                    HorizontalAlignment="Left"
                                    MinHeight="{StaticResource GeneralMinHeight}"/>
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_PatternStyle" Margin="0 10 0 5"/>
                            <telerik:RadDropDownButton x:Name="PatternStyleSelector" Width="153" Padding="0" HorizontalAlignment="Left" MinHeight="{StaticResource GeneralMinHeight}">
                                <telerik:RadDropDownButton.DropDownContent>
                                    <telerik:RadListBox x:Name="PatternListBox"
                                            Width="190"
                                            BorderThickness="0"
                                            ItemsSource="{Binding Path=PatternTypes}"
                                            SelectedIndex="{Binding Path=PatternType, Mode=TwoWay, Converter={StaticResource PatternTypeToIntConverter}}"
                                            ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                            ScrollViewer.VerticalScrollBarVisibility="Hidden"
                                            Height="115">
                                        <telerik:RadListBox.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <telerik:RadWrapPanel
                                                        Width="{Binding (FrameworkElement.ActualWidth), RelativeSource={RelativeSource AncestorType=ScrollContentPresenter}}"
                                                        Height="{Binding (FrameworkElement.ActualHeight), RelativeSource={RelativeSource AncestorType=ScrollContentPresenter}}"
                                                        VerticalAlignment="Top"
                                                        Orientation="Horizontal"/>
                                            </ItemsPanelTemplate>
                                        </telerik:RadListBox.ItemsPanel>
                                        <telerik:RadListBox.ItemTemplate>
                                            <DataTemplate>
                                                <spreadsheetControls:PatternFillControl
                                                        spreadsheetControls:RadControlsExtensions.ShouldCloseParentPopupOnClick="True"
                                                        Height="25"
                                                        Width="25"
                                                        ColorScheme="{Binding ElementName=PatternStyleSelector, Path=DataContext.ColorPalette.ColorScheme}"
                                                        PatternType="{Binding}"
                                                        PatternColor="#FF000000"
                                                        BackgroundColor="#FFFFFFFF"/>
                                            </DataTemplate>
                                        </telerik:RadListBox.ItemTemplate>
                                    </telerik:RadListBox>
                                </telerik:RadDropDownButton.DropDownContent>
                                <telerik:RadDropDownButton.Content>
                                    <spreadsheetControls:PatternFillControl x:Name="PatternDropDown"
                                            Width="170"
                                            PatternColor="{Binding Path=PatternColor, Converter={StaticResource ColorToThemableColorConverter}}"
                                            BackgroundColor="#FFFFFFFF"
                                            ColorScheme="{Binding Path=ColorPalette.ColorScheme}"
                                            PatternType="{Binding Path=PatternType}"
                                            Height="36"/>
                                </telerik:RadDropDownButton.Content>
                            </telerik:RadDropDownButton>
                        </StackPanel>
                        <telerik:GroupBox Grid.Row="1" Grid.ColumnSpan="2" Margin="2" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatCellsDialog_Fill_Sample">
                            <Grid>
                                <spreadsheetControls:PatternFillControl x:Name="SamplePattern"
                                        Height="40"
                                        Width="489"
                                        Margin="5"
                                        ColorScheme="{Binding Path=ColorPalette.ColorScheme, RelativeSource={RelativeSource TemplatedParent}}"
                                        PatternType="{Binding Path=PatternType, RelativeSource={RelativeSource TemplatedParent}}"
                                        PatternColor="{Binding Path=PatternColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToThemableColorConverter}}"
                                        BackgroundColor="{Binding Path=BackgroundColor, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource ColorToThemableColorConverter}}"
                                        Visibility="{Binding Path=State, Converter={StaticResource FillTabStateToVisibilityConverter}, ConverterParameter=PatternFillControl}"/>
                                <spreadsheetControls:GradientFillControl x:Name="SampleGradient"
                                        Height="40"
                                        Width="489"
                                        Margin="5"
                                        Color1="{Binding Path=Color1, Converter={StaticResource ColorToThemableColorConverter}}"
                                        Color2="{Binding Path=Color2, Converter={StaticResource ColorToThemableColorConverter}}"
                                        GradientType="{Binding Path=GradientType, RelativeSource={RelativeSource TemplatedParent}}"
                                        ColorScheme="{Binding Path=ColorPalette.ColorScheme, RelativeSource={RelativeSource TemplatedParent}}"
                                        Visibility="{Binding Path=State, Converter={StaticResource FillTabStateToVisibilityConverter}, ConverterParameter=GradientFillControl}"/>
                            </Grid>
                        </telerik:GroupBox>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
