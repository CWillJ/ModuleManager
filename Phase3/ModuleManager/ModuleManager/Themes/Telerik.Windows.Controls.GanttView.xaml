                                FontStyle="Normal"
                                FontWeight="Normal"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                            <Run Text="{StaticResource GlyphArrowDown}"/>
                        </TextBlock>
                        <TextBlock
                                Visibility="{Binding OriginalEvent.IsExpired, Converter={StaticResource InvertedBooleanToVisibilityConverter}}"
