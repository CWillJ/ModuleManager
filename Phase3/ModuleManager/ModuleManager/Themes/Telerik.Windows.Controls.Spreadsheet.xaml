    <Style x:Key="InsertChartDialogContentStyle" TargetType="dialogs:InsertChartDialogContent">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="WindowStartupLocation" Value="CenterScreen"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="Width" Value="800"/>
        <Setter Property="Height" Value="600"/>
