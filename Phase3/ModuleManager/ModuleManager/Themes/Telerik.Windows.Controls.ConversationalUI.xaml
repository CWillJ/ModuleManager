        <Setter Property="Wind" Value="{Binding Wind, Mode=TwoWay}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type chat:WeatherCard}">
                    <Grid Background="{TemplateBinding Background}" Margin="{TemplateBinding Padding}">
