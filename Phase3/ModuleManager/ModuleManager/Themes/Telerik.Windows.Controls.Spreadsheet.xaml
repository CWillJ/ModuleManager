        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <telerik:RadDropDownButton x:Name="PART_DropDownButton"
                            Command="{Binding InitializePopupCommand}"
                            IsOpen="{Binding Path=IsPopupOpened, Mode=TwoWay}"
