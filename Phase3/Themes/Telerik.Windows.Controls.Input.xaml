        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadColorPaletteViewItem">
                    <Grid x:Name="RootElement" SnapsToDevicePixels="True">
                        <ContentPresenter
                            x:Name="Content"
                            MinWidth="11"
                            MinHeight="11"
                            Margin="1"
                            ToolTipService.ToolTip="{Binding}" />
