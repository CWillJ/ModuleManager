    <Style x:Key="HeaderFooterDialogContentStyle" TargetType="dialogs:HeaderFooterDialogContent">
        <Setter Property="Header" Value="{Binding DialogHeader}"/>
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="Padding" Value="5"/>
        <Setter Property="ResizeMode" Value="NoResize"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dialogs:HeaderFooterDialogContent">
                    <Grid Background="{TemplateBinding Background}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition MinHeight="32"/>
                        </Grid.RowDefinitions>
                        <Rectangle Fill="{telerik:MaterialResource ResourceKey=PrimaryBrush}" Height="{StaticResource DialogTabstripHeight}" VerticalAlignment="Top" HorizontalAlignment="Stretch"/>
                        <telerik:RadTabControl x:Name="TabControl" BorderThickness="0">
                            <telerik:RadTabItem
                                    Header="{Binding TabHeader}"
                                    Visibility="{Binding IsVisible, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    DataContext="{Binding FirstPageContent}"
                                    MinWidth="55"
                                    IsSelected="{Binding IsSelected}">
                                <dialogs:HeaderFooterDialogTabContent/>
                            </telerik:RadTabItem>
                            <telerik:RadTabItem
                                    Header="{Binding TabHeader}"
                                    Visibility="{Binding IsVisible, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    DataContext="{Binding DefaultContent}"
                                    MinWidth="55"
                                    IsSelected="{Binding IsSelected}">
                                <dialogs:HeaderFooterDialogTabContent/>
                            </telerik:RadTabItem>
                            <telerik:RadTabItem
                                    Header="{Binding TabHeader}"
                                    Visibility="{Binding IsVisible, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    DataContext="{Binding EvenPageContent}"
                                    MinWidth="55"
                                    IsSelected="{Binding IsSelected}">
                                <dialogs:HeaderFooterDialogTabContent/>
                            </telerik:RadTabItem>
                        </telerik:RadTabControl>
                        <Border Grid.Row="1" Background="{telerik:MaterialResource ResourceKey=MainBrush}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0 1 0 0">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <telerik:RadButton
                                        telerik:LocalizationManager.ResourceKey="Ok"
                                        Command="{Binding OKCommand}"
                                        VerticalAlignment="Center"
                                        MinWidth="100"
                                        Margin="8"
                                        Style="{StaticResource SpreadsheetFlatRadButtonStyle}"/>
                                <telerik:RadButton
                                        telerik:LocalizationManager.ResourceKey="Cancel"
                                        Command="{Binding CloseCommand}"
                                        MinWidth="100"
                                        Margin="0 8 8 8"
                                        Style="{StaticResource SpreadsheetFlatRadButtonStyle}"/>
                            </StackPanel>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
