    <ControlTemplate x:Key="RadButtonCloseControlTemplate" TargetType="telerik:RadButton">
        <Grid>
            <Border x:Name="BorderVisual"
                    CornerRadius="{TemplateBinding CornerRadius}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <mat:MaterialControl IsSmartClipped="True" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                <TextBlock x:Name="CloseIcon"
                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                        FontFamily="{StaticResource TelerikWebUI}"
                        Foreground="{TemplateBinding Foreground}"
                        FontSize="16"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center">
                    <Run Text="{StaticResource GlyphClose}"/>
                </TextBlock>
            </mat:MaterialControl>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="{telerik:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
