    <ControlTemplate x:Key="EventDecoratorContainerTemplate" TargetType="gantt:EventDecoratorContainer">
        <Grid Margin="0 6 0 5">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ItemTypeStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Summary"/>
                    <VisualState x:Name="Milestone">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="ResizeStart" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
