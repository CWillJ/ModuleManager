    <ControlTemplate x:Key="DependencyIndicatorTemplate" TargetType="gantt:DependencyIndicator">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="DragOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Fill">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Duration="0:0:0" Storyboard.TargetName="Visual" Storyboard.TargetProperty="Stroke">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0:0:0.1" To="1.5" Storyboard.TargetName="EllipseScale" Storyboard.TargetProperty="ScaleX"/>
                            <DoubleAnimation Duration="0:0:0.1" To="1.5" Storyboard.TargetName="EllipseScale" Storyboard.TargetProperty="ScaleY"/>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Visual" Storyboard.TargetProperty="Margin">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Thickness>-1 0</Thickness>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Ellipse x:Name="Visual"
                    Stroke="{telerik:MaterialResource ResourceKey=PrimaryFocusBrush}"
                    Fill="{telerik:MaterialResource ResourceKey=MainBrush}"
                    Cursor="Hand"
                    VerticalAlignment="Center"
                    RenderTransformOrigin="0.5 0.5"
                    Width="12"
                    Height="12">
                <Ellipse.RenderTransform>
                    <ScaleTransform x:Name="EllipseScale"/>
                </Ellipse.RenderTransform>
                <telerik:ToolTipManager.ToolTip>
                    <ToolTip ContentTemplate="{StaticResource DragRelationTooltipTemplate}"/>
                </telerik:ToolTipManager.ToolTip>
            </Ellipse>
        </Grid>
