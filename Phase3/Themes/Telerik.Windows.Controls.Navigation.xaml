                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="PossibleDockingPosition" Value="Bottom">
                            <Setter TargetName="MaximizeAreaLeft" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaRight" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="MaximizeAreaTop" Property="Visibility" Value="Visible" />
                            <Setter TargetName="MaximizeAreaBottom" Property="Visibility" Value="Collapsed" />
