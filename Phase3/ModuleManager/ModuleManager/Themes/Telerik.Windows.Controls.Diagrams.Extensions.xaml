                                                Foreground="{telerik1:MaterialResource ResourceKey=MarkerBrush}"
                                                VerticalAlignment="Center"/>
                                    </StackPanel>
                                </Border>
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
