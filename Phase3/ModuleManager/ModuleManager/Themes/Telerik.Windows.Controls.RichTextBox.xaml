    <ControlTemplate x:Key="RulerThumbControlTemplate" TargetType="telerikControlsRTBUI:RulerThumb">
        <Grid>
            <ToolTipService.ToolTip>
                <ToolTip Content="{TemplateBinding ToolTipText}"/>
            </ToolTipService.ToolTip>
            <Path x:Name="Indent"
                    Stretch="Fill"
                    Data="{StaticResource IndentIcon}"
                    Visibility="Collapsed"
                    RenderTransformOrigin="0.5 0.5"
                    Fill="{TemplateBinding Background}"
                    Height="14"
                    Width="13"
                    Stroke="{TemplateBinding Foreground}"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="FirstLineIndent"
                    Stretch="Fill"
                    Data="{StaticResource FirstLineIndentIcon}"
                    SnapsToDevicePixels="True"
                    RenderTransformOrigin="0.5 0.5"
                    Height="14"
                    Width="13"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Background}"
                    Stroke="{TemplateBinding Foreground}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Rectangle x:Name="LeftIndent"
                    Fill="{TemplateBinding Background}"
                    Visibility="Collapsed"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Height="8"
                    Width="13"
                    Margin="0 8 0 0"
                    Stroke="{TemplateBinding Foreground}"/>
            <Path x:Name="LeftTab"
                    Data="{StaticResource LeftTabIcon}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Stretch="Fill"
                    UseLayoutRounding="True"
                    Width="16"
                    Height="16"
                    SnapsToDevicePixels="True"/>
            <Path x:Name="RightTab"
                    Data="{StaticResource RightTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="16"
                    Height="16"
                    SnapsToDevicePixels="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"
                    Stretch="Fill"/>
            <Path x:Name="CenterTab"
                    Data="{StaticResource CenterTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="16"
                    Height="16"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    UseLayoutRounding="True"/>
            <Path x:Name="DecimalTab"
                    Data="{StaticResource DecimalTabIcon}"
                    Visibility="Collapsed"
                    Fill="{TemplateBinding Foreground}"
                    Width="16"
                    Height="16"
                    SnapsToDevicePixels="True"
                    Stretch="Fill"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
            <Path x:Name="BarTab"
                    Visibility="Collapsed"
                    Data="{StaticResource BarTabIcon}"
                    Fill="{TemplateBinding Foreground}"
                    UseLayoutRounding="True"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                    Stretch="Fill"
                    Width="8"
                    Height="16"
                    SnapsToDevicePixels="True"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="RulerThumbType" Value="LeftTabStop">
                <Setter Property="Visibility" TargetName="LeftTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="RightTabStop">
                <Setter Property="Visibility" TargetName="RightTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="CenterTabStop">
                <Setter Property="Visibility" TargetName="CenterTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="DecimalTabStop">
                <Setter Property="Visibility" TargetName="DecimalTab" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="Indent">
                <Setter Property="Visibility" TargetName="Indent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="FirstLineIndent">
                <Setter Property="Visibility" TargetName="FirstLineIndent" Value="Visible"/>
            </Trigger>
            <Trigger Property="RulerThumbType" Value="LeftIndent">
                <Setter Property="Visibility" TargetName="LeftIndent" Value="Visible"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RulerThumbBaseStyle" TargetType="telerikControlsRTBUI:RulerThumb">
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="Background" Value="{telerikControls:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="Cursor" Value="Arrow"/>
        <Setter Property="Margin" Value="-4 9 0 0"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource RulerThumbControlTemplate}"/>
    </Style>
    <Style x:Key="RulerThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}"/>
    <Style x:Key="HangingIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 15 0 0"/>
    </Style>
    <Style x:Key="FirstLineIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 0 0 0"/>
    </Style>
    <Style x:Key="LeftIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 15 0 0"/>
    </Style>
    <Style x:Key="RightIndentThumbStyle" TargetType="telerikControlsRTBUI:RulerThumb" BasedOn="{StaticResource RulerThumbBaseStyle}">
        <Setter Property="Margin" Value="-4 15 0 0"/>
    </Style>
    <Style TargetType="Thumb" x:Key="TableColumnThumbStyle">
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid Background="{TemplateBinding Background}" Margin="-3 3 0 0">
                        <Border Background="{telerik:MaterialResource ResourceKey=MainBrush}"/>
                        <Border
                                Height="1"
                                Margin="0 1 0 0"
                                VerticalAlignment="Top"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                Height="1"
                                Margin="0 3 0 0"
                                VerticalAlignment="Top"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                Height="1"
                                Margin="0 5 0 0"
                                VerticalAlignment="Top"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                Height="1"
                                Margin="0 7 0 0"
                                VerticalAlignment="Top"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="1 0 0 0"
                                Width="1"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="False"
                                SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="3 0 0 0"
                                Width="1"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                        <Border
                                HorizontalAlignment="Left"
                                Margin="5 0 0 0"
                                Width="1"
                                Background="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                UseLayoutRounding="True"
                                SnapsToDevicePixels="True"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="Thumb" x:Key="SectionColumnThumbStyle">
        <Setter Property="Cursor" Value="SizeWE"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="Foreground" Value="{telerikControls:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Width" Value="10"/>
        <Setter Property="Height" Value="24"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid Background="{TemplateBinding Background}" Width="{TemplateBinding Width}" Margin="-5 1 0 1">
                        <Border BorderBrush="{telerik:MaterialResource ResourceKey=DividerBrush}" BorderThickness="1">
                            <Path
                                    Data="M0,0 L3,0 L3,1 L1,1 L1,8 L0,8 z"
                                    HorizontalAlignment="Left"
                                    Height="8"
                                    Width="3"
                                    Margin="2"
                                    Stretch="Fill"
                                    UseLayoutRounding="False"
                                    VerticalAlignment="Top"
                                    Fill="{TemplateBinding Foreground}"/>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="DocumentRulerBaseStyle" TargetType="telerikControlsRTBUI:DocumentRulerBase">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="MarkersBrush" Value="{telerikControls:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="HorizontalThumbStyle" Value="{StaticResource HorizontalThumbStyle}"/>
        <Setter Property="VerticalThumbStyle" Value="{StaticResource VerticalThumbStyle}"/>
        <Setter Property="ThumbBackgroundStyle" Value="{StaticResource ThumbBackgroundStyle}"/>
        <Setter Property="TableColumnThumbStyle" Value="{StaticResource TableColumnThumbStyle}"/>
        <Setter Property="SectionColumnThumbStyle" Value="{StaticResource SectionColumnThumbStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:DocumentRulerBase">
                    <Grid x:Name="PART_LayoutRoot">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="36"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="36"/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <ContentPresenter Grid.Column="1" Grid.Row="1"/>
                        <Canvas x:Name="PART_HorizontalCanvas"
                                Grid.Column="1"
                                Background="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                            <Grid x:Name="PART_HorizontalRulerContainer" Height="24" Margin="0 6">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition Height="0"/>
                                </Grid.RowDefinitions>
                                <mat:Shadow
                                        Grid.RowSpan="2"
                                        ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"
                                        VerticalAlignment="Top"
                                        Background="{telerikControls:MaterialResource ResourceKey=MainBrush}"
                                        Height="24"/>
                                <Canvas>
                                    <Rectangle x:Name="PART_LeftMarginRectangle" Style="{StaticResource PageMarginStyle}" Height="24"/>
                                    <Rectangle x:Name="PART_PageHorizontalRectangle"
                                            Fill="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"
                                            Canvas.Left="0"
                                            Canvas.Top="0"
                                            Height="24"
                                            Stroke="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                            StrokeThickness="0"/>
                                    <Rectangle x:Name="PART_RightMarginRectangle" Style="{StaticResource PageMarginStyle}" Height="24"/>
                                </Canvas>
                                <Canvas x:Name="PART_HorizontalMarkersContainer"/>
                                <Canvas x:Name="PART_HorizontalThumbBackgroundsContainer"/>
                                <Canvas x:Name="PART_HorizontalThumbsContainer" Canvas.ZIndex="10">
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphFirstLineIndent"
                                            RulerThumbType="FirstLineIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_FirstLineIndent}"
                                            Style="{StaticResource FirstLineIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphHangingIndent"
                                            RulerThumbType="Indent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_HangingIndent}"
                                            Style="{StaticResource HangingIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphLeftIndent"
                                            RulerThumbType="LeftIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_LeftIndent}"
                                            Style="{StaticResource LeftIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_ParagraphRightIndent"
                                            Canvas.ZIndex="10"
                                            ToolTipText="{Binding Manager.Culture, Source={StaticResource localizationManager}, Converter={StaticResource cultureConverter}, ConverterParameter=Documents_DocumentRuler_RightIndent}"
                                            Style="{StaticResource RightIndentThumbStyle}"
                                            RulerThumbType="Indent"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_TabStopMarker" Canvas.ZIndex="0" Foreground="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_FirstLineIndentMarker" RulerThumbType="FirstLineIndent" Canvas.ZIndex="0" Style="{StaticResource FirstLineIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_IndentMarker" RulerThumbType="Indent" Canvas.ZIndex="0" Style="{StaticResource HangingIndentThumbStyle}"/>
                                    <telerikControlsRTBUI:RulerThumb x:Name="PART_LeftIndentMarker" RulerThumbType="LeftIndent" Canvas.ZIndex="0" Style="{StaticResource LeftIndentThumbStyle}"/>
                                </Canvas>
                                <Canvas x:Name="PART_TabStopMarkersContainer" Grid.Row="1" Height="5"/>
                            </Grid>
                        </Canvas>
                        <Canvas x:Name="PART_VerticalCanvas"
                                Grid.Row="1"
                                Background="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch">
                            <Grid Grid.Row="1" x:Name="PART_VerticalRulerContainer" Margin="6 0" Width="24">
                                <mat:Shadow
                                        ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}"
                                        Background="{telerikControls:MaterialResource ResourceKey=MainBrush}"
                                        Width="24"
                                        HorizontalAlignment="Left"/>
                                <Canvas>
                                    <Rectangle x:Name="PART_TopMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="24"/>
                                    <Rectangle x:Name="PART_PageVerticalRectangle"
                                            Fill="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"
                                            Canvas.Left="0"
                                            Canvas.Top="0"
                                            Width="24"
                                            Stroke="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                            StrokeThickness="1"/>
                                    <Rectangle x:Name="PART_BottomMarginRectangle" Style="{StaticResource PageMarginStyle}" Width="24"/>
                                </Canvas>
                                <Canvas x:Name="PART_VerticalMarkersContainer"/>
                                <Canvas x:Name="PART_VerticalThumbBackgroundsContainer"/>
                                <Canvas x:Name="PART_VerticalThumbsContainer"/>
                            </Grid>
                        </Canvas>
                        <Border
                                Background="{telerik:MaterialResource ResourceKey=AlternativeBrush}"
                                BorderBrush="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                BorderThickness="1"
                                Margin="-1 -1 0 -1">
                            <Border x:Name="PART_TabStops"
                                    Background="{telerik:MaterialResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik:MaterialResource ResourceKey=DividerBrush}"
                                    BorderThickness="1"
                                    Width="36"
                                    Height="36">
                                <telerikControlsRTBUI:RulerThumb x:Name="PART_RulerThumbTypeSelector" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0" IsHitTestVisible="False"/>
                            </Border>
                        </Border>
                        <Line x:Name="PART_MarkLine"
                                Grid.Row="1"
                                Grid.Column="1"
                                Stroke="{telerik:MaterialResource ResourceKey=IconBrush}"
                                StrokeThickness="1"
                                StrokeDashArray="1 1 1 1 1 3"
                                Margin="0 2 0 0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutModeSelectorStyle" TargetType="telerikControlsRTBUIStatusBar:LayoutModeSelector">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:LayoutModeSelector">
                    <StackPanel x:Name="LayoutRoot" Orientation="Horizontal" UseLayoutRounding="True">
                        <telerik:RadToggleButton Padding="0" Height="36" CornerRadius="0" x:Name="btnPrintLayout" IsTabStop="False" IsBackgroundVisible="False" VerticalAlignment="Center" Width="36" Focusable="False">
                            <TextBlock x:Name="IconLeft"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                <Run Text="{StaticResource GlyphAlignLeft}"/>
                            </TextBlock>
                        </telerik:RadToggleButton>
                        <telerik:RadToggleButton
                                Padding="0"
                                Height="36"
                                CornerRadius="0"
                                x:Name="btnWebLayout"
                                Margin="4 0 0 0"
                                IsTabStop="False"
                                IsBackgroundVisible="False"
                                VerticalAlignment="Center"
                                Width="36"
                                Focusable="False">
                            <TextBlock
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                <Run Text="{StaticResource GlyphGlobe}"/>
                            </TextBlock>
                        </telerik:RadToggleButton>
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="FlowDirection" Value="RightToLeft">
                            <Setter TargetName="IconLeft" Property="Text" Value="{StaticResource GlyphAlignRight}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="NonLinearSliderStyle" TargetType="telerikControlsRTBUIStatusBar:NonLinearSlider">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:NonLinearSlider">
                    <Grid x:Name="LayoutRoot">
                        <telerik:RadSlider x:Name="slider" Minimum="0" Maximum="1" LargeChange="0.05" SmallChange="0.005" VerticalAlignment="Center"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PercentComboBoxStyle" TargetType="telerikControlsRTBUIStatusBar:PercentComboBox">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:PercentComboBox">
                    <Grid x:Name="LayoutRoot">
                        <telerik:RadComboBox x:Name="comboPercent" IsEditable="True" SelectedIndex="3" IsTextSearchEnabled="False" VerticalAlignment="Center" Margin="0 3">
                            <telerik:RadComboBoxItem Content="25%"/>
                            <telerik:RadComboBoxItem Content="50%"/>
                            <telerik:RadComboBoxItem Content="75%"/>
                            <telerik:RadComboBoxItem Content="100%"/>
                            <telerik:RadComboBoxItem Content="150%"/>
                            <telerik:RadComboBoxItem Content="200%"/>
                            <telerik:RadComboBoxItem Content="500%"/>
                        </telerik:RadComboBox>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ZoomControllerStyle" TargetType="telerikControlsRTBUIStatusBar:ZoomController">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUIStatusBar:ZoomController">
                    <Grid x:Name="LayoutRoot">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="120"/>
                            <ColumnDefinition Width="4"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*" MinWidth="120"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <telerikControlsRTBUIStatusBar:PercentComboBox x:Name="comboZoom"/>
                        <telerik:RadButton CornerRadius="0" Height="36" Width="36" IsBackgroundVisible="False" Grid.Column="2" x:Name="buttonZoomOut" BorderThickness="0" Padding="0" Margin="6 0">
                            <TextBlock
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                <Run Text="{StaticResource GlyphMinus}"/>
                            </TextBlock>
                        </telerik:RadButton>
                        <telerikControlsRTBUIStatusBar:NonLinearSlider x:Name="sliderZoom" VerticalAlignment="Center" Grid.Column="3"/>
                        <telerik:RadButton CornerRadius="0" Height="36" Width="36" IsBackgroundVisible="False" Grid.Column="4" x:Name="buttonZoomIn" BorderThickness="0" Padding="0" Margin="6 0 0 0">
                            <TextBlock
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="16"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontWeight="Normal"
                                    FontStyle="Normal"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center">
                                <Run Text="{StaticResource GlyphPlus}"/>
                            </TextBlock>
                        </telerik:RadButton>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRichTextBoxStatusBarStyle" TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar">
        <Setter Property="FontFamily" Value="{telerikControls:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerikControls:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerikControls:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerikControls:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerikControls:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:RadRichTextBoxStatusBar">
                    <Border BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
                        <Grid x:Name="LayoutRoot" VerticalAlignment="Center" HorizontalAlignment="Right">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <telerikControlsRTBUIStatusBar:LayoutModeSelector x:Name="layoutModeSelector" Margin="0 0 6 0" VerticalAlignment="Center"/>
                            <telerikControlsRTBUIStatusBar:ZoomController x:Name="zoomController" Grid.Column="1" Margin="0 0 6 0"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ActionButtonStyle" TargetType="telerik:RadRibbonToggleButton" BasedOn="{StaticResource RadRibbonToggleButtonStyle}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
    </Style>
    <Style x:Key="MultipleUndoControlStyle" TargetType="telerikControlsRTBUI:MultipleUndoControl">
        <Setter Property="MinWidth" Value="120"/>
        <Setter Property="ActionButtonStyle" Value="{StaticResource ActionButtonStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:MultipleUndoControl">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <StackPanel x:Name="actionButtonsPanel"/>
                        <telerik:RadButton x:Name="textBlockMultipleUndoActionsContent"
                                Grid.Row="1"
                                Margin="{StaticResource MultipleUndoControl_ButtonMargin}"
                                ClickMode="Press"
                                BorderThickness="0 1 0 0"
                                HorizontalContentAlignment="Center"
                                MinHeight="36"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PasteOptionsPickerStyle" TargetType="telerikControlsRTBUI:PasteOptionsPicker">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="0"/>
        <Setter Property="Background" Value="{telerikControls:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerikControls:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="MinWidth" Value="150"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:PasteOptionsPicker">
                    <Grid Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.RowSpan="2"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"/>
                        <Border x:Name="HeaderBorder"
                                Background="{telerikControls:MaterialResource ResourceKey=AccentNormalBrush}"
                                BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}"
                                BorderBrush="Transparent">
                            <telerik:Label telerik:LocalizationManager.ResourceKey="Documents_PasteOptions_Name" FontWeight="Bold"/>
                        </Border>
                        <ItemsControl x:Name="PART_CommandsItemsControl"
                                DataContext="{x:Null}"
                                Grid.Row="1"
                                BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                BorderBrush="Transparent"
                                ItemsSource="{Binding AvailablePasteOptions}">
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Orientation="Horizontal"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Grid IsHitTestVisible="True">
                                        <ContentControl ToolTip="{Binding Tooltip}" Content="{Binding PasteOption}">
                                            <ContentControl.ContentTemplate>
                                                <DataTemplate>
                                                    <telerik:RadButton
                                                            mat:MaterialAssist.IsShadowDisabled="True"
                                                            IsBackgroundVisible="False"
                                                            Command="{Binding RelativeSource={RelativeSource AncestorType=telerikControlsRTBUI:PasteOptionsPicker}, Path=PasteCommand}"
                                                            CommandParameter="{Binding}">
                                                        <Image
                                                                Width="24"
                                                                Height="24"
                                                                HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"
                                                                Source="{telerik:IconResource IconRelativePathStringFormat='{}{0}.png', IconSources={StaticResource IconSourcesPaste}}"/>
                                                    </telerik:RadButton>
                                                </DataTemplate>
                                            </ContentControl.ContentTemplate>
                                        </ContentControl>
                                    </Grid>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                        </ItemsControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="PasteOptionsDropDownPickerStyle" TargetType="telerikControlsRTBUI:PasteOptionsDropDownPicker">
        <Setter Property="mat:MaterialAssist.CornerRadius" Value="0"/>
        <Setter Property="Background" Value="{telerikControls:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerikControls:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikControlsRTBUI:PasteOptionsDropDownPicker">
                    <telerik:RadDropDownButton x:Name="PART_DropDownButton">
                        <telerik:RadDropDownButton.Content>
                            <Image
                                    Width="16"
                                    Height="16"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Source="{telerik:IconResource IconRelativePath='16/paste.png', IconSources={StaticResource IconSources}}"/>
                        </telerik:RadDropDownButton.Content>
                        <telerik:RadDropDownButton.DropDownContent>
                            <Grid MinWidth="150" Margin="{TemplateBinding Padding}">
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Border x:Name="BorderVisual"
                                        Grid.RowSpan="2"
                                        Background="{TemplateBinding Background}"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="{TemplateBinding BorderThickness}"
                                        CornerRadius="{TemplateBinding mat:MaterialAssist.CornerRadius}"/>
                                <Border x:Name="HeaderBorder"
                                        Background="{telerikControls:MaterialResource ResourceKey=AccentNormalBrush}"
                                        BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftTopRight}"
                                        BorderBrush="Transparent">
                                    <telerik:Label Content="Paste Options:" FontWeight="Bold"/>
                                </Border>
                                <telerik:RadListBox x:Name="PART_CommandsListBox"
                                        Grid.Row="1"
                                        BorderThickness="{TemplateBinding BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=LeftRightBottom}"
                                        BorderBrush="Transparent"
                                        SelectedValuePath="PasteOption"
                                        SelectedValue="{Binding SelectedPasteOption, Mode=OneWay}"
                                        ItemsSource="{Binding AvailablePasteOptions}"
                                        SelectionMode="Single">
                                    <telerik:RadListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <StackPanel Orientation="Horizontal"/>
                                        </ItemsPanelTemplate>
                                    </telerik:RadListBox.ItemsPanel>
                                    <telerik:RadListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid IsHitTestVisible="True">
