    <ControlTemplate x:Key="ToggleButtonTemplate" TargetType="telerik:RadToggleButton">
        <Border
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            Background="Transparent">
            <Border
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                <Grid>
                    <TextBlock
                        x:Name="Plus"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        Foreground="{TemplateBinding Foreground}"
                        Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}">
                        <Run Text="{StaticResource GlyphPlus}" />
                    </TextBlock>
                    <TextBlock
                        x:Name="Minus"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        Foreground="{TemplateBinding Foreground}"
                        Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                        Visibility="Collapsed">
                        <Run Text="{StaticResource GlyphMinus}" />
                    </TextBlock>
                </Grid>
            </Border>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="Plus" Property="Opacity" Value="1" />
                <Setter TargetName="Minus" Property="Opacity" Value="1" />
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter TargetName="Plus" Property="Opacity" Value="1" />
                <Setter TargetName="Minus" Property="Opacity" Value="1" />
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}" />
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="Plus" Property="Visibility" Value="Collapsed" />
                <Setter TargetName="Minus" Property="Visibility" Value="Visible" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="PropertyGridNestedToggleButtonStyle" TargetType="telerik:RadToggleButton">
