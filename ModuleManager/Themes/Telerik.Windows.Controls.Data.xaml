            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}" />
            </Trigger>
        </Style.Triggers>
