                                Margin="0,0,16,0"
                                Focusable="True"
                                IsTabStop="True"
                                TabIndex="0" />
                        </StackPanel>
                        <StackPanel
                            x:Name="DialogButtons"
                            Grid.Row="1"
                            Grid.ColumnSpan="2"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <telerik:RadButton
                                x:Name="OK"
                                MinWidth="74"
                                Margin="0,8,8,8"
                                HorizontalAlignment="Right"
                                telerik:LocalizationManager.ResourceKey="Ok"
                                telerikNavigation:RadWindow.ResponseButton="Accept"
                                Command="{x:Static telerik:WindowCommands.Confirm}"
                                IsTabStop="True"
                                Style="{StaticResource RadWindowDialogsFlatRadButtonStyle}"
                                TabIndex="1" />
                            <telerik:RadButton
                                x:Name="Cancel"
                                MinWidth="74"
                                Margin="0,8,8,8"
                                HorizontalAlignment="Right"
                                telerik:LocalizationManager.ResourceKey="Cancel"
                                telerikNavigation:RadWindow.ResponseButton="Cancel"
                                Command="{x:Static telerik:WindowCommands.Cancel}"
                                IsTabStop="True"
                                Style="{StaticResource RadWindowDialogsFlatRadButtonStyle}"
                                TabIndex="2" />
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadConfirmStyle" TargetType="telerikNavigation:RadConfirm">
