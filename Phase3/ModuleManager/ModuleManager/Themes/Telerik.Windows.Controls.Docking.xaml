                        </ContentControl>
                        <dock:RadGridResizer x:Name="Resizer" Background="#01010101" Placement="Bottom" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Grid.Row="1" Height="6" Cursor="SizeNS"/>
