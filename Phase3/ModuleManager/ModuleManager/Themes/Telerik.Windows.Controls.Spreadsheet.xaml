        <Setter Property="GalleryContentItemStyle" Value="{StaticResource StyleGalleryContentItemStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:StyleGallery">
                    <Grid Width="{TemplateBinding QuickStylesWidth}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ListBox x:Name="QuickStyles"
                                    mat:MaterialAssist.ShadowDepth="Depth0"
                                    ScrollViewer.HorizontalScrollBarVisibility="Hidden"
