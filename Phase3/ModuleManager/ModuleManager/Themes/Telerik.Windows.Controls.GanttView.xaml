            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="34"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup>
                    <VisualState x:Name="HighlightedState"/>
                    <VisualState x:Name="SelectedState"/>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="PART_ExpandCollapseHitArea" MinHeight="36" Background="Transparent" Grid.Column="0" HorizontalAlignment="Stretch">
                <TextBlock x:Name="PART_ExpandCollapseVisual"
