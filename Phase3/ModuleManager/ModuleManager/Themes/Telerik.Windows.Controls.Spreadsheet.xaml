                                            Grid.Column="1"
                                            Margin="0 3"
                                            ItemsSource="{Binding Path=WithinValues, Mode=OneWay}"
                                            DisplayMemberPath="Name"
                                            SelectedValuePath="SearchLocation"
                                            SelectedValue="{Binding Path=SearchWithinValue, Mode=TwoWay}"/>
                                    <telerik:Label Grid.Row="1" telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_Search"/>
                                    <telerik:RadComboBox
                                            Grid.Row="1"
                                            Grid.Column="1"
                                            Margin="0 3"
                                            ItemsSource="{Binding Path=SearchBy, Mode=OneWay}"
                                            DisplayMemberPath="Name"
                                            SelectedValuePath="SearchLocation"
                                            SelectedValue="{Binding Path=SearchByType, Mode=TwoWay}"/>
                                    <telerik:Label Grid.Row="2" telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_LookIn"/>
                                    <telerik:RadComboBox
                                            Grid.Row="2"
                                            Grid.Column="1"
                                            Margin="0 3"
                                            ItemsSource="{Binding Path=LookInValues, Mode=OneWay}"
                                            DisplayMemberPath="Name"
                                            SelectedValuePath="SearchLocation"
                                            SelectedValue="{Binding Path=LookIn, Mode=TwoWay}"/>
