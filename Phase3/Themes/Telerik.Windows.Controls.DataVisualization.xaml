    <Style x:Key="TimelineItemControlStyle" TargetType="timelines:TimelineItemControl">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{telerik1:MaterialResource ResourceKey=AccentPressedBrush}"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="Height" Value="{StaticResource TimelineItemDimensions}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Margin" Value="0 5"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=ComplementaryBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineItemControl">
                    <Grid>
                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
                        <Border x:Name="OuterBorder" Background="#01FFFFFF">
                            <Rectangle x:Name="timelineItem" Fill="{TemplateBinding Background}"/>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsHovered" Value="True">
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True"/>
                                <Condition Property="IsHovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineInstantItemControlStyle" TargetType="timelines:TimelineInstantItemControl">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{telerik1:MaterialResource ResourceKey=AccentPressedBrush}"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="Margin" Value="0 5"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=AccentHoverBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineInstantItemControl">
                    <Grid Height="{StaticResource TimelineItemDimensions}" Background="#01FFFFFF" Width="{StaticResource TimelineItemDimensions}" RenderTransformOrigin=".5 .5">
                        <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}" Height="16" Width="16"/>
                        <Rectangle x:Name="timelineItem" Height="16" HorizontalAlignment="Center" VerticalAlignment="Center" Width="16" Fill="{TemplateBinding Background}"/>
                        <Grid.RenderTransform>
                            <RotateTransform Angle="45"/>
                        </Grid.RenderTransform>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsHovered" Value="True">
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=AccentPressedBrush}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True"/>
                                <Condition Property="IsHovered" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="timelineItem" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=AccentPressedBrush}"/>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <DataTemplate x:Key="TimelineItemTemplate">
        <timelines:TimelineItemControl/>
    </DataTemplate>
    <DataTemplate x:Key="TimelineInstantItemTemplate">
        <timelines:TimelineInstantItemControl/>
    </DataTemplate>
    <Style x:Key="TimelineAnnotationStyle" TargetType="timelines:TimelineAnnotation">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="Margin" Value="0 22 0 0"/>
        <Setter Property="Canvas.ZIndex" Value="300"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:TimelineAnnotation">
                    <ContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SelectionRegionStyle" TargetType="timelines:SelectionRegion">
        <Setter Property="Visibility" Value="Collapsed"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="timelines:SelectionRegion">
                    <Grid>
                        <Rectangle Stroke="{TemplateBinding BorderBrush}" StrokeDashArray="3 3"/>
                        <Rectangle Fill="{TemplateBinding Background}" Opacity="0.5" Margin="1"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TimelineItemContainerStyle" TargetType="timelines:TimelineItemContainer">
        <Setter Property="Canvas.ZIndex" Value="200"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <timelines:TimelineItemGroupsPanel Padding="0 43 0 4" VerticalScrollBar="{Binding ElementName=Part_VerticalSlider}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadTimelineStyle" TargetType="datavizControls:RadTimeline">
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="Padding" Value="0 0 0 2"/>
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1"/>
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="SliderStyle" Value="{StaticResource Timeline_SliderStyle}"/>
        <Setter Property="TimelineItemTemplate" Value="{StaticResource TimelineItemTemplate}"/>
        <Setter Property="TimelineInstantItemTemplate" Value="{StaticResource TimelineInstantItemTemplate}"/>
        <Setter Property="PeriodHeaderStyle" Value="{StaticResource TimelinePeriodHeaderStyle}"/>
        <Setter Property="GroupPeriodHeaderStyle" Value="{StaticResource TimelineGroupPeriodHeaderStyle}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="datavizControls:RadTimeline">
                    <Border
                            Margin="{TemplateBinding Padding}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            SnapsToDevicePixels="True"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}">
                        <Grid Background="{TemplateBinding Background}" Margin="{TemplateBinding Padding}">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <mat:Shadow Grid.ColumnSpan="2" Grid.RowSpan="2" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
                            <Border
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(timeBars:ActualSizeProxy.ActualHeight)}"
                                    VerticalAlignment="Bottom"
                                    Background="{TemplateBinding Background}"/>
                            <timelines:TimelineAnnotationsPanel x:Name="PART_TimelineAnnotationsPanel" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Slider="{Binding ElementName=PART_Slider}">
                                <Grid Canvas.ZIndex="100">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <timelines:TimelineStripLineContainer x:Name="PART_TimelineStripLineContainer" Grid.Row="2" timeBars:ActualSizeProxy.IsEnabled="True" ItemsSource="{TemplateBinding IntervalItems}">
                                        <timelines:TimelineStripLineContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:VirtualizingTimeBarPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineStripLineContainer.ItemsPanel>
                                        <timelines:TimelineStripLineContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelineStripLineContainer.Template>
                                    </timelines:TimelineStripLineContainer>
                                    <timeBars:TimeSpecialSlotContainer x:Name="PART_TimeSpecialSlotContainer" Grid.Row="2" ItemsSource="{TemplateBinding SpecialSlots}">
                                        <timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:TimeBarPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timeBars:TimeSpecialSlotContainer.ItemsPanel>
                                        <timeBars:TimeSpecialSlotContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timeBars:TimeSpecialSlotContainer.Template>
                                    </timeBars:TimeSpecialSlotContainer>
                                    <Border Grid.Row="1" Style="{StaticResource TimelineLineStyle}"/>
                                    <timelines:TimelineGroupPeriodContainer x:Name="PART_TimelineGroupPeriodContainer" Grid.Row="0" ItemsSource="{TemplateBinding IntervalGroups}" ItemHeaderStyle="{TemplateBinding GroupPeriodHeaderStyle}">
                                        <timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelineGroupPeriodContainer.ItemsPanel>
                                        <timelines:TimelineGroupPeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelineGroupPeriodContainer.Template>
                                    </timelines:TimelineGroupPeriodContainer>
                                    <timelines:TimelinePeriodContainer x:Name="PART_TimelinePeriodContainer"
                                            Grid.Row="1"
                                            Grid.RowSpan="2"
                                            ItemsSource="{TemplateBinding IntervalItems}"
                                            ItemHeaderStyle="{TemplateBinding PeriodHeaderStyle}">
                                        <timelines:TimelinePeriodContainer.ItemsPanel>
                                            <ItemsPanelTemplate>
                                                <timeBars:LabelMeasurementPanel Slider="{Binding ElementName=PART_Slider}"/>
                                            </ItemsPanelTemplate>
                                        </timelines:TimelinePeriodContainer.ItemsPanel>
                                        <timelines:TimelinePeriodContainer.Template>
                                            <ControlTemplate>
                                                <ItemsPresenter/>
                                            </ControlTemplate>
                                        </timelines:TimelinePeriodContainer.Template>
                                    </timelines:TimelinePeriodContainer>
                                </Grid>
                                <timelines:TimelineItemContainer x:Name="PART_TimelineItemContainer"
                                        GroupExpandMode="{TemplateBinding GroupExpandMode}"
                                        ItemsSource="{TemplateBinding GroupedDataItems}"
                                        SelectionRegionControl="{Binding ElementName=PART_SelectionRegion}">
                                    <timelines:TimelineItemContainer.Template>
                                        <ControlTemplate>
                                            <Grid Background="#01FFFFFF">
                                                <ItemsPresenter/>
                                            </Grid>
                                        </ControlTemplate>
                                    </timelines:TimelineItemContainer.Template>
                                </timelines:TimelineItemContainer>
                            </timelines:TimelineAnnotationsPanel>
                            <timelines:SelectionRegionPanel x:Name="PART_SelectionRegionPanel" Grid.Row="0" Grid.Column="0" Slider="{Binding ElementName=PART_Slider}">
                                <timelines:SelectionRegion x:Name="PART_SelectionRegion"/>
                            </timelines:SelectionRegionPanel>
                            <Border
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    MinWidth="36"
                                    Visibility="{Binding Visibility, ElementName=Part_VerticalSlider}"
                                    Background="{telerik1:MaterialResource ResourceKey=ComplementaryBrush}"/>
                            <timelines:TimelineScrollBar x:Name="Part_VerticalSlider"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    VerticalAlignment="Bottom"
                                    Visibility="Collapsed"
                                    ScrollBarVisibility="{TemplateBinding VerticalScrollBarVisibility}"
                                    Style="{TemplateBinding SliderStyle}"
                                    Height="{Binding ElementName=PART_TimelineStripLineContainer, Path=(timeBars:ActualSizeProxy.ActualHeight)}"
                                    Minimum="0"
                                    Maximum="1"
                                    SelectionStart="0"
                                    SelectionEnd="1"
                                    Orientation="Vertical"
                                    IsDirectionReversed="True"
                                    ThumbVisibility="Collapsed"/>
                            <telerik1:RadSlider x:Name="PART_Slider"
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Style="{TemplateBinding SliderStyle}"
                                    Width="Auto"
                                    Minimum="{Binding Path=PeriodStart, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                    Maximum="{Binding Path=PeriodEnd, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource dateConverter}}"
                                    MinimumRangeSpan="{Binding Path=ActualMinZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    MaximumRangeSpan="{Binding Path=MaxZoomRange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timeSpanToMaximumRangeSpanConverter}}"
                                    Selection="{Binding Path=ActualVisiblePeriodRange, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource selectionRangeConverter}}"
                                    SmallChange="{Binding Path=SmallChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    LargeChange="{Binding Path=LargeChange, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource timespanConverter}}"
                                    ThumbVisibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToThumbVisibilityConverter}}"
                                    Visibility="{Binding Path=ScrollMode, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource scrollModeToVisibilityConverter}}"
                                    IsCoercionSuppressed="True"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="timelines:TimelineItemControl" BasedOn="{StaticResource TimelineItemControlStyle}"/>
    <Style TargetType="timelines:TimelineInstantItemControl" BasedOn="{StaticResource TimelineInstantItemControlStyle}"/>
    <Style TargetType="timelines:TimelineAnnotation" BasedOn="{StaticResource TimelineAnnotationStyle}"/>
    <Style TargetType="timelines:SelectionRegion" BasedOn="{StaticResource SelectionRegionStyle}"/>
    <Style TargetType="timelines:TimelineItemContainer" BasedOn="{StaticResource TimelineItemContainerStyle}"/>
    <Style TargetType="datavizControls:RadTimeline" BasedOn="{StaticResource RadTimelineStyle}"/>
</ResourceDictionary>
