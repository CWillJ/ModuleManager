    <ControlTemplate x:Key="RadRibbonGroupDropDownButtonTemplate" TargetType="telerikRibbonView:RadRibbonDropDownButton">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="KeyboardNavigationVisual" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderVisual"
                    Background="{TemplateBinding Background}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Border x:Name="CheckVisual"
                    Opacity="0"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{x:Null}"
                    Background="{TemplateBinding mat:MaterialAssist.CheckedBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <Border x:Name="KeyboardNavigationVisual"
                    Opacity="0"
                    Background="{telerikControls:MaterialResource ResourceKey=AlternativeBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    CornerRadius="{TemplateBinding CornerRadius}"/>
            <mat:MaterialControl CornerRadius="{TemplateBinding CornerRadius}" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" IsSmartClipped="True">
                <Grid Margin="{TemplateBinding Padding}" x:Name="ContentGrid" VerticalAlignment="Center">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ContentPresenter x:Name="ContentPresenter"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            RecognizesAccessKey="True"/>
                    <Border x:Name="Image" Margin="12 0 12 24" Width="32" Height="32">
                        <Image VerticalAlignment="Center" HorizontalAlignment="Center" Width="16" Height="16" Source="{TemplateBinding SmallImage}" Stretch="None"/>
                    </Border>
                    <TextBlock Name="textRow1" Grid.Row="1" Text="{TemplateBinding TextRow1}" TextAlignment="Center" Foreground="{TemplateBinding Foreground}"/>
                    <StackPanel Name="DropDownPanel" Grid.Row="2" HorizontalAlignment="Center" Orientation="Horizontal">
                        <TextBlock Name="textRow2" Text="{TemplateBinding TextRow2}" Foreground="{TemplateBinding Foreground}"/>
                        <TextBlock x:Name="DropDownIndicator"
                                Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                VerticalAlignment="Top"
                                FontSize="16"
                                Foreground="{telerikControls:MaterialResource ResourceKey=IconBrush}"
                                Opacity="{telerikControls:MaterialResource ResourceKey=SecondaryOpacity}"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontFamily="{StaticResource TelerikWebUI}"
                                Margin="2"
                                HorizontalAlignment="Center"
                                FontStyle="Normal"
                                FontWeight="Normal">
                            <Run x:Name="DropDownIndicatorRun" Text="{StaticResource GlyphArrow60Down}"/>
                        </TextBlock>
                    </StackPanel>
                </Grid>
            </mat:MaterialControl>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                <Grid Margin="5 0 5 5">
                    <mat:Shadow x:Name="PopupShadow"
                            ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=OneWay, Path=(mat:Shadow.ShadowDepth)}"
                            Height="{TemplateBinding DropDownHeight}"
                            Width="{TemplateBinding DropDownWidth}"
                            SnapsToDevicePixels="True"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{telerikControls:MaterialResource ResourceKey=MainBrush}"
                            CornerRadius="{telerikControls:MaterialResource ResourceKey=CornerRadiusBottom}"/>
                    <Border x:Name="DropDownPopupBorder"
                            Padding="0"
                            Margin="0"
                            MinWidth="3"
                            MinHeight="3"
                            Width="{TemplateBinding DropDownWidth}"
                            Height="{TemplateBinding DropDownHeight}"
                            MaxWidth="{TemplateBinding DropDownMaxWidth}"
                            MaxHeight="{TemplateBinding DropDownMaxHeight}"
                            Background="{telerikControls:MaterialResource ResourceKey=MainBrush}"
                            BorderBrush="{telerikControls:MaterialResource ResourceKey=DividerBrush}"
                            CornerRadius="{telerikControls:MaterialResource ResourceKey=CornerRadiusBottom}"
                            BorderThickness="0 1 0 0">
                        <ContentControl x:Name="DropDownPopupContent"
                                Content="{TemplateBinding DropDownContent}"
                                ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                VerticalContentAlignment="Stretch"
                                DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}"
                                FocusVisualStyle="{x:Null}"/>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="ContentPresenter" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow2" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter TargetName="DropDownIndicatorRun" Property="Text" Value="{StaticResource GlyphArrow60Up}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="True"/>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="Background" TargetName="BorderVisual" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="True"/>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="True"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="0" To="1" Storyboard.TargetName="CheckVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.EnterActions>
                <MultiTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="1" To="0" Storyboard.TargetName="CheckVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.ExitActions>
            </MultiTrigger>
            <Trigger Property="DropDownButtonPosition" Value="Right">
                <Setter TargetName="DropDownPanel" Property="Grid.Column" Value="2"/>
                <Setter TargetName="DropDownPanel" Property="Grid.RowSpan" Value="3"/>
                <Setter TargetName="DropDownPanel" Property="Grid.Row" Value="0"/>
                <Setter TargetName="DropDownPanel" Property="Margin" Value="2 0 0 0"/>
                <Setter TargetName="textRow1" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="Image" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="ContentPresenter" Property="Grid.RowSpan" Value="2"/>
                <Setter TargetName="textRow2" Property="Visibility" Value="Collapsed"/>
            </Trigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                <Setter TargetName="DropDownIndicator" Property="VerticalAlignment" Value="Center"/>
                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter TargetName="ContentGrid" Property="Margin" Value="12 0"/>
                <Setter TargetName="Image" Property="Margin" Value="0"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
