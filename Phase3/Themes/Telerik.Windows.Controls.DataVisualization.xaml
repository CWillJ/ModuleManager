                        <Trigger Property="IsDragSelected" Value="True">
                            <Setter TargetName="content" Property="Fill" Value="{telerik1:MaterialResource ResourceKey=AccentHoverBrush}"/>
                            <Setter TargetName="label" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
                            <Setter TargetName="label" Property="Opacity" Value="1"/>
                            <Setter TargetName="border" Property="Background" Value="{telerik1:MaterialResource ResourceKey=AccentNormalBrush}"/>
                            <Setter TargetName="border" Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=AccentNormalBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
