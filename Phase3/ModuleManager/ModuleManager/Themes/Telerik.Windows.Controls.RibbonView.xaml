        <Setter Property="Template" Value="{StaticResource RadRibbonViewTemplate}"/>
        <Style.Resources>
            <Style TargetType="telerikInput:RadColorPicker" BasedOn="{StaticResource RadRibbonColorPickerStyle}"/>
        </Style.Resources>
    </Style>
