                                            FontFamily="{StaticResource TelerikWebUI}"
                                            FontSize="16"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center">
                                        <Run Text="{StaticResource GlyphArrow60Right}"/>
                                    </TextBlock>
                                    <TextBlock x:Name="ExpandedVisual"
                                            Visibility="Collapsed"
                                            SnapsToDevicePixels="True"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            FontSize="16"
                                            FontWeight="Normal"
                                            FontStyle="Normal"
                                            Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                            FontFamily="{StaticResource TelerikWebUI}">
                                        <Run Text="{StaticResource GlyphArrow60Down}"/>
                                    </TextBlock>
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="IsChecked" Value="True">
                                        <Setter TargetName="ExpandedVisual" Property="Visibility" Value="Visible"/>
                                        <Setter TargetName="CollapsedVisual" Property="Visibility" Value="Collapsed"/>
