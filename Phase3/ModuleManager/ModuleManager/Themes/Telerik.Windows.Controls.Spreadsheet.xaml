            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Content" Property="Opacity" Value="{telerik:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="ExpandIconRight" Property="Opacity" Value="{telerik:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="Tick" Property="Opacity" Value="{telerik:MaterialResource ResourceKey=DisabledOpacity}"/>
