                                FontFamily="{StaticResource TelerikWebUI}"
                                Foreground="{telerik:MaterialResource ResourceKey=IconBrush}"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center">
