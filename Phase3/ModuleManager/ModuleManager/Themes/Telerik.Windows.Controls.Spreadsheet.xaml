                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MinWidth="50" Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                </Grid.RowDefinitions>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Width" VerticalAlignment="Center"/>
                                    <telerik:RadNumericUpDown
                                            Grid.Column="1"
                                            Value="{Binding CommonWidth, Mode=TwoWay, TargetNullValue=''}"
                                            Minimum="0.1"
                                            CustomUnit="{Binding Path=RegionalUnitString}"
                                            SmallChange="0.1"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommand="{Binding Path=SetShapeWidthCommand}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Value}"
                                            Width="90"
                                            Margin="3 3 0 3"/>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Height" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                    <telerik:RadNumericUpDown
                                            Grid.Column="1"
                                            Value="{Binding CommonHeight, Mode=TwoWay, TargetNullValue=''}"
                                            Minimum="0.1"
                                            SmallChange="0.1"
                                            CustomUnit="{Binding Path=RegionalUnitString}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommand="{Binding Path=SetShapeHeightCommand}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Value}"
                                            Width="90"
                                            Margin="3 3 0 3"/>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition MinWidth="50" Width="Auto"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Rotation" VerticalAlignment="Center"/>
                                    <telerik:RadNumericUpDown
                                            Grid.Column="1"
                                            Value="{Binding Rotation, Mode=TwoWay, TargetNullValue=''}"
                                            Minimum="-3600"
                                            Maximum="3600"
                                            CustomUnit="°"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommand="{Binding Path=SetShapeRotationCommand}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Value}"
                                            Width="90"
                                            Margin="3 3 0 3"/>
                                </Grid>
                            </Grid>
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Scale" Margin="0 5" Style="{StaticResource HeaderTextBlockStyle}"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition MinWidth="50" Width="Auto"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition MinWidth="50" Width="Auto"/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Width" VerticalAlignment="Center"/>
                                    <telerik:RadNumericUpDown
                                            Grid.Column="1"
                                            Value="{Binding WidthScale, Mode=TwoWay, TargetNullValue=''}"
                                            Minimum="1"
                                            SmallChange="1"
                                            CustomUnit="%"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommand="{Binding Path=SetScaleWidthCommand}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Value}"
                                            Width="90"
                                            Margin="3 3 0 3"/>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Height" VerticalAlignment="Center" HorizontalAlignment="Right"/>
                                    <telerik:RadNumericUpDown
                                            Grid.Column="1"
                                            Value="{Binding HeightScale, Mode=TwoWay, TargetNullValue=''}"
                                            Minimum="1"
                                            SmallChange="1"
                                            CustomUnit="%"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommand="{Binding Path=SetScaleHeightCommand}"
                                            spreadsheetControls:RadNumericUpDownExtensions.UIInteractionCommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=Value}"
                                            Width="90"
                                            Margin="3 3 0 3"/>
                                </Grid>
                            </Grid>
                            <CheckBox
                                    Command="{Binding Path=ChangeLockAspectRatioCommand}"
                                    CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked}"
                                    telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_LockAspectRatio"
                                    IsChecked="{Binding InitialLockAspectRatio, Mode=OneTime}"
                                    Margin="5 10 5 5"/>
                            <CheckBox
                                    Command="{Binding Path=ChangeRelativeToOriginalResizeCommand}"
                                    CommandParameter="{Binding RelativeSource={RelativeSource Self}, Path=IsChecked}"
                                    telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_Relative"
                                    IsChecked="{Binding InitialRelativeToOriginalResize, Mode=OneTime}"
                                    IsEnabled="{Binding RelativeToOriginalResizeIsEnabled, Mode=OneTime}"
                                    Margin="5 0 5 5"/>
                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_OriginalSize" Margin="0 5" Style="{StaticResource HeaderTextBlockStyle}"/>
                            <StackPanel Orientation="Horizontal">
                                <TextBlock Visibility="{Binding Path=SelectionHasImages, Converter={StaticResource BoolToVisibilityValueConverter}}" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_OriginalWidth" Margin="0 2 5 2"/>
                                <TextBlock Visibility="{Binding Path=SelectionHasImages, Converter={StaticResource BoolToVisibilityValueConverter}}" Text="{Binding OriginalWidth, Mode=OneTime}" Margin="0 2 5 2"/>
                                <TextBlock Visibility="{Binding Path=SelectionHasImages, Converter={StaticResource BoolToVisibilityValueConverter}}" telerik:LocalizationManager.ResourceKey="Spreadsheet_FormatShapesDialog_OriginalHeight" Margin="5 2"/>
                                <TextBlock Visibility="{Binding Path=SelectionHasImages, Converter={StaticResource BoolToVisibilityValueConverter}}" Text="{Binding OriginalHeight, Mode=OneTime}" Margin="0 2 5 2"/>
                            </StackPanel>
                        </StackPanel>
                        <Border Grid.Row="1" Background="{telerik:MaterialResource ResourceKey=MainBrush}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="0 1 0 0">
                            <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                <telerik:RadButton
                                        telerik:LocalizationManager.ResourceKey="Reset"
                                        Visibility="{Binding Path=SelectionHasImages, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                        Command="{Binding Path=ResetCommand}"
                                        VerticalAlignment="Center"
                                        MinWidth="100"
                                        Margin="8"
                                        Style="{StaticResource SpreadsheetFlatRadButtonStyle}"/>
                                <telerik:RadButton
                                        Command="{Binding Path=CloseCommand}"
                                        telerik:LocalizationManager.ResourceKey="Close"
                                        MinWidth="100"
                                        Margin="0 8 8 8"
                                        Style="{StaticResource SpreadsheetFlatRadButtonStyle}"/>
                            </StackPanel>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
