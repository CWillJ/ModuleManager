                        <Trigger SourceName="PaneToggleButton" Property="IsMouseOver" Value="True">
                            <Setter TargetName="PaneToggleButton" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
