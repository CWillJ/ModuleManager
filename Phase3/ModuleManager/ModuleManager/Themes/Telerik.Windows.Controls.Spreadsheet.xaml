    <Style x:Key="StyleGalleryContentItemStyle" TargetType="ListBoxItem">
        <Setter Property="Padding" Value="4 3"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">
                    <Grid Background="{TemplateBinding Background}">
                        <telerik:RadContextMenu.ContextMenu>
                            <telerik:RadContextMenu x:Name="PART_ContextMenu">
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_StyleGallery_Apply" Command="{Binding Path=ApplyStyleCommand, Mode=OneTime}" CommandParameter="{Binding Path=ContentItem, Mode=OneTime}"/>
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_StyleGallery_Modify" Command="{Binding Path=ModifyStyleCommand, Mode=OneTime}" CommandParameter="{Binding Path=ContentItem, Mode=OneTime}"/>
                                <telerik:RadMenuItem telerik:LocalizationManager.ResourceKey="Spreadsheet_StyleGallery_Duplicate" Command="{Binding Path=DuplicateStyleCommand, Mode=OneTime}" CommandParameter="{Binding Path=ContentItem, Mode=OneTime}"/>
                                <telerik:RadMenuItem
                                        telerik:LocalizationManager.ResourceKey="Spreadsheet_StyleGallery_Delete"
                                        Command="{Binding Path=DeleteStyleCommand, Mode=OneTime}"
                                        CommandParameter="{Binding Path=ContentItem, Mode=OneTime}"
                                        IsEnabled="{Binding Path=ContentItem.CellStyle.IsRemoveable, Mode=OneTime}"/>
                            </telerik:RadContextMenu>
                        </telerik:RadContextMenu.ContextMenu>
                        <ToolTipService.ToolTip>
                            <ToolTip Content="{Binding Path=ContentItem.CellStyle.Name, Mode=OneTime}"/>
                        </ToolTipService.ToolTip>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To=".50" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="MouseOverVisualElement"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0.8" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="contentPresenter"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="SelectionStates">
                                <VisualState x:Name="Unselected"/>
                                <VisualState x:Name="Selected">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To=".75" Storyboard.TargetProperty="Opacity" Storyboard.TargetName="SelectedRectangle"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Duration="0" Storyboard.TargetProperty="Visibility" Storyboard.TargetName="FocusVisualElement">
                                            <DiscreteObjectKeyFrame KeyTime="0">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Unfocused"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle x:Name="SelectedRectangle" Fill="{telerik:MaterialResource ResourceKey=PrimaryPressedBrush}" IsHitTestVisible="False" Opacity="0"/>
                        <Rectangle x:Name="FocusVisualElement" Fill="{telerik:MaterialResource ResourceKey=PrimaryFocusBrush}" Visibility="Collapsed"/>
                        <Rectangle x:Name="MouseOverVisualElement" Fill="{telerik:MaterialResource ResourceKey=PrimaryHoverBrush}" IsHitTestVisible="False" Opacity="0"/>
                        <Rectangle Fill="#FFFFFFFF" IsHitTestVisible="False" Margin="{TemplateBinding Padding}"/>
                        <Image x:Name="contentPresenter"
                                Source="{Binding Path=ContentItem.ImageSource}"
                                Stretch="None"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Margin="{TemplateBinding Padding}"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="StyleGalleryStyle" TargetType="spreadsheetControls:StyleGallery">
