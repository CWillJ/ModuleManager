    <Style x:Key="RadToolBarStyle" TargetType="telerikNavigation:RadToolBar">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}" />
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="MinWidth" Value="48" />
        <Setter Property="MinHeight" Value="48" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="Margin" Value="1" />
        <Setter Property="UseLayoutRounding" Value="True" />
        <Setter Property="FocusManager.IsFocusScope" Value="True" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikNavigation:ToolBarContainerStyleSelector>
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource RadToolBarSeparatorStyle}" TypeName="RadToolBarSeparator" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarTextBlockStyle}" TypeName="TextBlock" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarTextBoxStyle}" TypeName="TextBox" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarButtonStyle}" TypeName="Button" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRepeatButtonStyle}" TypeName="RepeatButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarToggleButtonStyle}" TypeName="ToggleButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarCheckBoxStyle}" TypeName="CheckBox" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadioButtonStyle}" TypeName="RadioButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadButtonStyle}" TypeName="RadButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadPathButtonStyle}" TypeName="RadPathButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadToggleButtonStyle}" TypeName="RadToggleButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadRadioButtonStyle}" TypeName="RadRadioButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadDropDownButtonStyle}" TypeName="RadDropDownButton" />
                    <telerikNavigation:ToolBarContainerStyle ContainerStyle="{StaticResource ToolBarRadSplitButtonStyle}" TypeName="RadSplitButton" />
                </telerikNavigation:ToolBarContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBar">
                    <Grid x:Name="RootElement" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0" Width="Auto" />
                            <ColumnDefinition x:Name="c1" Width="*" />
                            <ColumnDefinition x:Name="c2" Width="Auto" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0" Height="Auto" />
                            <RowDefinition x:Name="r1" Height="*" />
                            <RowDefinition x:Name="r2" Height="Auto" />
                        </Grid.RowDefinitions>
                        <mat:Shadow
                            Grid.RowSpan="3"
                            Grid.ColumnSpan="3"
                            Background="{TemplateBinding Background}"
                            ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                        <Border
                            x:Name="BorderVisual"
                            Grid.RowSpan="3"
                            Grid.ColumnSpan="3"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}" />
                        <telerik:RadToggleButton
                            x:Name="PART_DropdownButton"
                            Grid.RowSpan="2"
                            Grid.Column="2"
                            IsChecked="{TemplateBinding IsOverflowOpen}"
                            IsEnabled="{TemplateBinding HasOverflowItems}"
                            Style="{StaticResource ToolBarOverflowButtonStyle}"
                            Visibility="{TemplateBinding OverflowButtonVisibility}">
                            <TextBlock
                                x:Name="arrow"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontFamily="{StaticResource TelerikWebUI}"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}">
                                <Run Text="{StaticResource GlyphArrowOverflowDown}" />
                            </TextBlock>
                        </telerik:RadToggleButton>
                        <Grid x:Name="PopupGrid" Grid.Column="2">
                            <Popup
                                x:Name="PART_Overflow"
                                AllowsTransparency="True"
                                Focusable="false"
                                HorizontalOffset="0"
                                IsOpen="{Binding Path=IsOverflowOpen, RelativeSource={RelativeSource TemplatedParent}}"
                                Placement="Bottom"
                                PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}"
                                StaysOpen="False"
                                VerticalOffset="1">
                                <Grid Margin="5">
                                    <mat:Shadow
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadius}"
                                        ShadowDepth="Depth2" />
                                    <Border
                                        MinWidth="55"
                                        MinHeight="26"
                                        Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                        BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                        BorderThickness="0"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadius}">
                                        <telerikToolBar:RadToolBarOverflowPanel
                                            x:Name="PART_OverflowPanel"
                                            VerticalAlignment="Top"
                                            Orientation="Vertical" />
                                    </Border>
                                </Grid>
                            </Popup>
                        </Grid>
                        <Grid Background="Transparent" Visibility="{TemplateBinding GripVisibility}">
                            <TextBlock
                                x:Name="GripOrnamentH"
                                HorizontalAlignment="Left"
                                VerticalAlignment="Center"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontFamily="{StaticResource TelerikWebUI}"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                Visibility="Visible">
                                <Run Text="{StaticResource GlyphMoreVertical}" />
                            </TextBlock>
                            <TextBlock
                                x:Name="GripOrnamentV"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Top"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                FontFamily="{StaticResource TelerikWebUI}"
                                FontSize="16"
                                FontStyle="Normal"
                                FontWeight="Normal"
                                Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                Visibility="Collapsed">
                                <Run Text="{StaticResource GlyphMoreHorizontal}" />
                            </TextBlock>
                        </Grid>
                        <telerikToolBar:RadToolBarPanel
                            x:Name="PART_StripPanel"
                            Grid.Column="1"
                            Margin="{TemplateBinding Padding}"
                            Orientation="Horizontal" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Orientation" Value="Horizontal">
                            <Setter TargetName="c0" Property="Width" Value="Auto" />
                            <Setter TargetName="c1" Property="Width" Value="*" />
                            <Setter TargetName="c2" Property="Width" Value="Auto" />
                            <Setter TargetName="r0" Property="Height" Value="*" />
                            <Setter TargetName="r1" Property="Height" Value="Auto" />
                            <Setter TargetName="r2" Property="Height" Value="Auto" />
                        </Trigger>
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*" />
                            <Setter TargetName="c1" Property="Width" Value="Auto" />
                            <Setter TargetName="c2" Property="Width" Value="Auto" />
                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1" />
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2" />
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0" />
                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible" />
                            <Setter TargetName="PopupGrid" Property="Grid.Row" Value="2" />
                            <Setter TargetName="PopupGrid" Property="Grid.Column" Value="0" />
                            <Setter TargetName="PART_Overflow" Property="Placement" Value="Right" />
                            <Setter TargetName="PART_Overflow" Property="HorizontalOffset" Value="1" />
                            <Setter TargetName="PART_Overflow" Property="VerticalOffset" Value="0" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="GripOrnamentH" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                            <Setter TargetName="GripOrnamentV" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadToolBarTrayStyle" TargetType="telerikNavigation:RadToolBarTray">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Padding" Value="0" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <telerikToolBar:RadToolBarTrayPanel x:Name="PART_BandPanel" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBarTray">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                        <ItemsPresenter x:Name="PART_ItemsPresenter" Margin="{TemplateBinding Padding}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
