                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                        <Setter Property="Content">
                                            <Setter.Value>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    FontSize="16"
                                                    FontStyle="Normal"
                                                    FontWeight="Normal">
                                                    <Run Text="{StaticResource GlyphArrow60Right}" />
                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </telerik:RadButton.Style>
                    </telerik:RadButton>

                    <!--  NumericPresenter  -->
                    <Border VerticalAlignment="Center" Visibility="{Binding PagerControlsVisibility.NumericElementsControlVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                        <dataPager:NumericElementsPresenter
                            AutoEllipsisMode="{TemplateBinding AutoEllipsisMode}"
                            NumericButtonCount="{TemplateBinding NumericButtonCount}"
                            NumericButtonStyle="{TemplateBinding NumericButtonStyle}"
                            PageCount="{TemplateBinding PageCount}"
                            PageIndex="{TemplateBinding PageIndex}" />
                    </Border>

                    <!--  NextPage Button  -->
                    <telerik:RadButton
                        Name="MoveToNextPageButton"
                        Command="{x:Static controls:RadDataPagerCommands.MoveToNextPage}"
                        Visibility="{Binding PagerControlsVisibility.MoveToNextPageControlVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                        <telerik:RadButton.Style>
                            <Style BasedOn="{StaticResource PageButtonStyle}" TargetType="{x:Type telerik:RadButton}">
                                <Style.Triggers>
                                    <Trigger Property="FlowDirection" Value="LeftToRight">
                                        <Setter Property="Content">
                                            <Setter.Value>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    FontSize="16"
                                                    FontStyle="Normal"
                                                    FontWeight="Normal">
                                                    <Run Text="{StaticResource GlyphArrow60Right}" />
                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                        <Setter Property="Content">
                                            <Setter.Value>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    FontSize="16"
                                                    FontStyle="Normal"
                                                    FontWeight="Normal">
                                                    <Run Text="{StaticResource GlyphArrow60Left}" />
                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </telerik:RadButton.Style>
                    </telerik:RadButton>

                    <!--  LastPage Button  -->
                    <telerik:RadButton
                        Name="MoveToLastPageButton"
                        Command="{x:Static controls:RadDataPagerCommands.MoveToLastPage}"
                        Visibility="{Binding PagerControlsVisibility.MoveToLastPageControlVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                        <telerik:RadButton.Style>
                            <Style BasedOn="{StaticResource PageButtonStyle}" TargetType="{x:Type telerik:RadButton}">
                                <Style.Triggers>
                                    <Trigger Property="FlowDirection" Value="LeftToRight">
                                        <Setter Property="Content">
                                            <Setter.Value>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    FontSize="16"
                                                    FontStyle="Normal"
                                                    FontWeight="Normal">
                                                    <Run Text="{StaticResource GlyphArrowForward}" />
                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                    <Trigger Property="FlowDirection" Value="RightToLeft">
                                        <Setter Property="Content">
                                            <Setter.Value>
                                                <TextBlock
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center"
                                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                                    FontFamily="{StaticResource TelerikWebUI}"
                                                    FontSize="16"
                                                    FontStyle="Normal"
                                                    FontWeight="Normal">
                                                    <Run Text="{StaticResource GlyphArrowBackward}" />
                                                </TextBlock>
                                            </Setter.Value>
                                        </Setter>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </telerik:RadButton.Style>
                    </telerik:RadButton>
                </StackPanel>
                <Grid
                    Grid.Column="1"
                    Margin="4,0,0,0"
                    Background="Transparent"
                    Visibility="{Binding PagerControlsVisibility.TextControlVisibility, RelativeSource={RelativeSource TemplatedParent}}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="auto" />
                        <ColumnDefinition Width="auto" />
                    </Grid.ColumnDefinitions>
                    <TextBlock
                        Margin="4,0"
                        VerticalAlignment="Center"
                        telerik:LocalizationManager.ResourceKey="RadDataPagerPage"
                        TextAlignment="Center" />
                    <dataPager:DataPagerTextBox
                        Grid.Column="1"
                        MinWidth="36"
                        MinHeight="36"
                        VerticalAlignment="Center"
                        Command="{x:Static controls:RadDataPagerCommands.MoveToPage}"
                        PageIndex="{TemplateBinding PageIndex}" />
                    <TextBlock
                        Grid.Column="2"
                        Margin="4,0"
                        VerticalAlignment="Center"
                        telerik:LocalizationManager.ResourceKey="RadDataPagerOf"
                        TextAlignment="Center" />
                    <TextBlock
                        Grid.Column="3"
                        Margin="4,0"
                        VerticalAlignment="Center"
                        Text="{Binding PageCount, RelativeSource={RelativeSource TemplatedParent}}"
                        TextAlignment="Center" />
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="DataPagerPresenterStyle" TargetType="dataPager:DataPagerPresenter">
        <Setter Property="Template" Value="{StaticResource DataPagerPresenterTemplate}" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}" />
