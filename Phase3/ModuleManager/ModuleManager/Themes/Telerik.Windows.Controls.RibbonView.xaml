            <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding Color}"/>
            <Border Background="{TemplateBinding Color}"/>
            <ContentControl
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    IsTabStop="False"
                    Content="{TemplateBinding Header}"
                    FontSize="{TemplateBinding FontSize}"
                    Opacity="{telerik1:MaterialResource ResourceKey=PrimaryOpacity}"
                    HorizontalAlignment="{TemplateBinding HeaderAlignment}"
                    Foreground="{TemplateBinding Foreground}"
                    Margin="{TemplateBinding Padding}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="ContextualGroupsStyle" TargetType="telerikRibbonView:RadRibbonContextualGroup">
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="Margin" Value="0 0 3 0"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="BorderThickness" Value="0 3 0 0"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="HeaderAlignment" Value="Center"/>
        <Setter Property="Padding" Value="5 5 5 2"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Template" Value="{StaticResource ContextualGroupsTemplate}"/>
    </Style>
    <Style x:Key="WindowTitleStyle" TargetType="telerikRibbonViewPrimitives:WindowTitle">
        <Setter Property="Margin" Value="0 6 40 13"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="IsHitTestVisible" Value="false"/>
        <Setter Property="IsActive" Value="True"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikRibbonViewPrimitives:WindowTitle">
                    <telerikRibbonViewPrimitives:WindowTitlePanel>
                        <TextBlock x:Name="Title"
                                Text="{TemplateBinding Title}"
                                FontSize="{telerik1:MaterialResource ResourceKey=FontSize}"
                                Foreground="{TemplateBinding Foreground}"
                                TextTrimming="CharacterEllipsis"/>
                        <TextBlock x:Name="Divider"
                                Text="{telerik:LocalizableResource Key=RibbonViewWindowTitleDivider}"
                                Visibility="{TemplateBinding DividerVisibility}"
                                Foreground="{TemplateBinding Foreground}"
                                FontSize="{telerik1:MaterialResource ResourceKey=FontSize}"/>
                        <TextBlock x:Name="ApplicationName"
                                Text="{TemplateBinding ApplicationName}"
                                Foreground="{TemplateBinding Foreground}"
                                TextTrimming="CharacterEllipsis"
                                FontSize="{telerik1:MaterialResource ResourceKey=FontSize}"/>
                    </telerikRibbonViewPrimitives:WindowTitlePanel>
                    <ControlTemplate.Triggers>
                        <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=IsHostedInRibbonWindow, FallbackValue=False}" Value="True">
                            <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"/>
                        </DataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=IsHostedInRibbonWindow, FallbackValue=False}" Value="True"/>
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonWindow}, Path=IsWindowsThemeEnabled}" Value="True"/>
                                <Condition Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonWindow}, Path=IsTitleVisible}" Value="False"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Foreground" Value="White"/>
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="HelpButtonStyle" TargetType="telerikRibbonView:RadRibbonButton" BasedOn="{StaticResource RadRibbonButtonStyle}">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"/>
        <Setter Property="MinWidth" Value="36"/>
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <ControlTemplate x:Key="MinimizeButtonTemplate" TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Grid x:Name="RootElement">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="KeyboardNavigationStates">
                    <VisualState x:Name="KeyboardUnfocused"/>
                    <VisualState x:Name="KeyboardFocused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="KeyboardNavigationVisual"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="OuterBorder"
                    CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadius}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding Background}"/>
            <Border x:Name="KeyboardNavigationVisual"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    Opacity="0"
                    Background="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"
                    BorderBrush="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
            <mat:MaterialControl CornerRadius="{TemplateBinding CornerRadius}" IsSmartClipped="True">
                <TextBlock x:Name="MinimizeButtonContent"
                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Center"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontWeight="Normal"
                        FontStyle="Normal"
                        Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                        FontSize="16"
                        Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                        Margin="{TemplateBinding Padding}">
                    <Run x:Name="MinimizeButtonGlyph" Text="{StaticResource GlyphArrowChevronUp}"/>
                </TextBlock>
            </mat:MaterialControl>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="MinimizeButtonContent" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="MinimizeButtonGlyph" Property="Text" Value="{StaticResource GlyphUnpin}"/>
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewExpandRibbon}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="False">
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewMinimizeRibbon}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="MinimizeButtonStyle" TargetType="telerikRibbonView:RadRibbonToggleButton">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Size" Value="Small"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="MinWidth" Value="16"/>
        <Setter Property="MinHeight" Value="16"/>
        <Setter Property="Padding" Value="8"/>
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=ComplementaryBrush}"/>
        <Setter Property="mat:MaterialAssist.CheckedBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="Template" Value="{StaticResource MinimizeButtonTemplate}"/>
    </Style>
    <ControlTemplate x:Key="RadRibbonViewTemplate" TargetType="telerikRibbonView:RadRibbonView">
        <Grid x:Name="RootPanel" Margin="{TemplateBinding Padding}">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <mat:Shadow Grid.Row="1" Grid.RowSpan="2" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
            <Grid Grid.Row="1">
                <Grid.Resources>
                    <Style x:Key="SelectedTabContentStyle" TargetType="ContentPresenter">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}"/>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Default">
                                <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}"/>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Grid.Resources>
                <ContentPresenter x:Name="SelectedTabContent" Style="{StaticResource SelectedTabContentStyle}"/>
            </Grid>
            <Grid x:Name="TopGrid">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Border x:Name="titleBarBackground" Background="{TemplateBinding TitleBarBackground}" IsHitTestVisible="False" Grid.ColumnSpan="3"/>
                <Rectangle Grid.Row="1" Grid.ColumnSpan="3" Fill="{TemplateBinding TitleBarBackground}"/>
                <telerikRibbonViewPrimitives:RibbonTitleBarPanel x:Name="RibbonTitleBarPanel" Grid.ColumnSpan="2" Visibility="{TemplateBinding TitleBarVisibility}">
                    <ContentPresenter x:Name="AboveToolBarHost" VerticalAlignment="Center" telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="QuickAccessToolBar"/>
                    <telerikRibbonViewPrimitives:WindowTitle x:Name="WindowTitle"
                            Title="{TemplateBinding Title}"
                            telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="WindowTitle"
                            ApplicationName="{TemplateBinding ApplicationName}"
                            Style="{TemplateBinding WindowTitleStyle}"/>
                    <StackPanel x:Name="ContextualGroupsHeaders" Margin="1 0 0 -4" Orientation="Horizontal" telerikRibbonViewPrimitives:RibbonTitleBarPanel.Element="ContextualGroupsPanel"/>
                </telerikRibbonViewPrimitives:RibbonTitleBarPanel>
                <telerikRibbonViewPrimitives:RadRibbonApplicationDropDownButton x:Name="ApplicationButton"
                        Grid.Row="1"
                        IsEnabled="{TemplateBinding IsApplicationButtonEnabled}"
                        IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsApplicationMenuOpen, Mode=TwoWay}"
                        SmallImage="{TemplateBinding ApplicationButtonImageSource}"
                        Content="{TemplateBinding ApplicationButtonContent}"
                        Visibility="{TemplateBinding ApplicationButtonVisibility}"
                        Style="{TemplateBinding ApplicationButtonStyle}"/>
                <telerikRibbonViewPrimitives:RibbonScrollViewer
                        Grid.Column="1"
                        Grid.Row="1"
                        x:Name="TabsScrollViewer"
                        mat:MaterialAssist.IsShadowDisabled="True"
                        mat:MaterialAssist.MouseOverBrush="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"
                        mat:MaterialAssist.PressedBrush="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"
                        BorderBrush="Transparent"
                        BorderThickness="0"
                        Foreground="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}"
                        Background="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}">
                    <ItemsPresenter/>
                </telerikRibbonViewPrimitives:RibbonScrollViewer>
                <StackPanel Orientation="Horizontal" Grid.Column="2" Grid.Row="1">
                    <telerik:RadToggleSwitchButton x:Name="LayoutModeButton"
                            VerticalAlignment="Center"
                            ContentPosition="Right"
                            Foreground="{TemplateBinding Foreground}"
                            CheckedContent="{TemplateBinding LayoutModeButtonContent}"
                            UncheckedContent="{TemplateBinding LayoutModeButtonContent}"
                            IsChecked="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode , Converter={StaticResource BooleanToRibbonLayoutConverter}, Mode=TwoWay}"
                            HorizontalAlignment="Center"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=ShowLayoutModeButton, Converter={StaticResource RibbonBooleanToVisibilityConverter}}"/>
                    <telerikRibbonView:RadRibbonButton
                            Command="{TemplateBinding HelpCommand}"
                            CommandParameter="{TemplateBinding HelpCommandParameter}"
                            CommandTarget="{TemplateBinding HelpCommandTarget}"
                            x:Name="HelpButton"
                            Visibility="{TemplateBinding HelpButtonVisibility}"
                            SmallImage="{TemplateBinding HelpButtonImageSource}"
                            Style="{TemplateBinding HelpButtonStyle}"
                            telerikRibbonView:ScreenTip.Title="{telerik:LocalizableResource Key=RibbonViewHelp}"/>
                    <ContentPresenter Content="{TemplateBinding TabStripAdditionalContent}"/>
                </StackPanel>
                <Rectangle Fill="Transparent" VerticalAlignment="Bottom" Height="2" Grid.ColumnSpan="3" Grid.RowSpan="2"/>
            </Grid>
            <ContentPresenter x:Name="BelowToolBarHost" Grid.Row="2"/>
            <Popup x:Name="TabContentPopup" PlacementTarget="{Binding ElementName=TopGrid}" AllowsTransparency="True">
                <Grid Margin="0 0 0 5" x:Name="PopupGrid">
                    <Grid.Resources>
                        <Style x:Key="SelectedTabContentPopupStyle" TargetType="ContentPresenter">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Simplified">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=SimplifiedContentHeight}"/>
                                </DataTrigger>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=LayoutMode}" Value="Default">
                                    <Setter Property="Height" Value="{Binding RelativeSource={RelativeSource AncestorType=telerikRibbonView:RadRibbonView}, Path=ContentHeight}"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </Grid.Resources>
                    <mat:Shadow x:Name="PopupShadow"
                            SnapsToDevicePixels="True"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}"/>
                    <ContentPresenter x:Name="SelectedTabContentPopup" Style="{StaticResource SelectedTabContentPopupStyle}"/>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsHostedInRibbonWindow" Value="True">
                <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadRibbonColorPickerStyle" TargetType="telerikInput:RadColorPicker" BasedOn="{StaticResource RadColorPickerStyle}">
        <Setter Property="SplitButtonStyle">
            <Setter.Value>
                <Style TargetType="telerik:RadSplitButton" BasedOn="{StaticResource RadSplitButtonStyle}">
                    <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True"/>
                    <Setter Property="IsBackgroundVisible" Value="True"/>
                    <Setter Property="mat:MaterialAssist.FocusBrush" Value="{x:Null}"/>
                </Style>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRibbonViewStyle" TargetType="telerikRibbonView:RadRibbonView">
        <Setter Property="ItemsPanel">
