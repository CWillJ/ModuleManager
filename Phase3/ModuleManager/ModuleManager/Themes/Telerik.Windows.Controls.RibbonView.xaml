                    </StackPanel>
                </Grid>
            </mat:MaterialControl>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                <Grid Margin="5 0 5 5">
                    <mat:Shadow x:Name="PopupShadow"
                            ShadowDepth="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=OneWay, Path=(mat:Shadow.ShadowDepth)}"
