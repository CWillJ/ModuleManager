                                        <Run Text="{StaticResource GlyphWindow}"/>
                                    </TextBlock>
                                </Grid>
                                <Grid Visibility="Collapsed" x:Name="Collapse" Background="Transparent" ToolTipService.ToolTip="Restore Down" HorizontalAlignment="Center" VerticalAlignment="Center">
