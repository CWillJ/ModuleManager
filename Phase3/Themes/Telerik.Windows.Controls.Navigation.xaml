                            <MultiTrigger.Conditions>
                                <Condition Property="IsPaneOpen" Value="True" />
                                <Condition Property="DisplayMode" Value="Compact" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_DismissPaneOverlay" Property="Visibility" Value="Visible" />
