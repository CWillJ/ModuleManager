                                            Margin="0 0 8 0"
                                            Style="{StaticResource SpreadsheetFlatRadButtonStyle}"
                                            Command="{Binding Path=FindNextCommand}"
                                            VerticalAlignment="Center"
                                            MinWidth="100"/>
                                </StackPanel>
                                <StackPanel Grid.Column="2" Orientation="Horizontal" Margin="8 0 0 0">
                                    <telerik:RadButton
                                            telerik:LocalizationManager.ResourceKey="Close"
                                            Margin="0 0 8 0"
                                            Command="{Binding Path=CloseDialogCommand}"
                                            VerticalAlignment="Center"
                                            MinWidth="100"
                                            Style="{StaticResource SpreadsheetFlatRadButtonStyle}"/>
                                </StackPanel>
                            </Grid>
                        </Border>
                        <StackPanel Grid.Row="3" Visibility="{Binding ResultsGridVisibility}">
                            <grid:RadGridView x:Name="ResultsGridView"
                                    ItemsSource="{Binding Path=FindAllResults}"
                                    RowIndicatorVisibility="Collapsed"
                                    IsReadOnly="True"
                                    AutoGenerateColumns="False"
                                    CanUserFreezeColumns="False"
                                    ShowGroupPanel="False"
                                    VerticalAlignment="Top"
                                    MaxWidth="548"
                                    MaxHeight="277"
                                    Margin="1"
                                    BorderThickness="1">
                                <grid:RadGridView.Columns>
                                    <grid:GridViewColumn MinWidth="75">
                                        <grid:GridViewColumn.Header>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_SheetColumn"/>
                                        </grid:GridViewColumn.Header>
                                        <grid:GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                        Content="{Binding Path=FoundCell.Worksheet.Name}"
                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=grid:RadGridView} , Path=DataContext.GoToCellCommand}"
                                                        CommandParameter="{Binding Path=FoundCell}"
                                                        Style="{StaticResource LinkButtonStyle}"/>
                                            </DataTemplate>
                                        </grid:GridViewColumn.CellTemplate>
                                    </grid:GridViewColumn>
                                    <grid:GridViewColumn MinWidth="75">
                                        <grid:GridViewColumn.Header>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_Cell"/>
                                        </grid:GridViewColumn.Header>
                                        <grid:GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
