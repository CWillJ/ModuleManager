                            <telerik:RadColorPaletteView x:Name="RecentColorsPalette"
                                    Grid.Row="7"
                                    Margin="0 0 0 1"
                                    BorderThickness="0"
                                    Background="Transparent"
                                    BorderBrush="Transparent"
                                    Visibility="Collapsed"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                    VerticalAlignment="Center"
                                    MinHeight="14">
                                <telerik:RadColorPaletteView.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerik:RadUniformGrid Background="Transparent" Columns="10" Rows="1"/>
                                    </ItemsPanelTemplate>
                                </telerik:RadColorPaletteView.ItemsPanel>
                            </telerik:RadColorPaletteView>
                            <Border x:Name="AdditionalContentBorder" Grid.Row="8" Visibility="Collapsed" Margin="0 1 0 0" BorderBrush="Transparent" BorderThickness="0">
                                <ContentPresenter Margin="0 1 0 0" Content="{TemplateBinding AdditionalContent}" TextElement.Foreground="{TemplateBinding Foreground}"/>
                            </Border>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ColorSelectorDropDownStyle" TargetType="spreadsheetControls:ColorSelectorDropDown">
