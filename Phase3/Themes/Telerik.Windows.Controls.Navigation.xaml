            <Setter.Value>
                <ControlTemplate TargetType="treeView:TreeViewDragVisual">
                    <Grid Margin="3">
                        <Grid.RowDefinitions>
                            <RowDefinition />
                            <RowDefinition />
                        </Grid.RowDefinitions>
                        <Border
                            x:Name="DragBackgroundVisual"
