                                BorderThickness="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=BorderThickness, Converter={StaticResource ThicknessToOrientedThicknessConverter}, ConverterParameter=Right}"
                                CanContentScroll="{TemplateBinding ScrollViewer.CanContentScroll}"
