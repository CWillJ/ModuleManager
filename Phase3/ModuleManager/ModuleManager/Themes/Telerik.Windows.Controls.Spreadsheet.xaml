                                        </ItemsPanelTemplate>
                                    </telerik:RadListBox.ItemsPanel>
                                </telerik:RadListBox>
                            </StackPanel>
                        </Grid>
                        <StackPanel Grid.Row="2" Orientation="Horizontal" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="10">
                            <telerik:RadButton Content="OK" Margin="10 0" MinWidth="60" x:Name="PART_ButtonOK"/>
                            <telerik:RadButton Content="Cancel" MinWidth="60" x:Name="PART_ButtonCancel"/>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
