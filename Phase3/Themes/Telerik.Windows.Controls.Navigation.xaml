                <ControlTemplate TargetType="layoutControl:LayoutControlSelectionIndicator">
                    <Grid x:Name="GridRoot">
                        <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                            <Grid>
                                <Border Background="{TemplateBinding Background}" Opacity="0.5" />
                                <Grid
                                    x:Name="LockPathGrid"
                                    Width="64"
                                    Height="64"
                                    Opacity="0">
                                    <Ellipse Stroke="{telerik1:MaterialResource ResourceKey=MainBrush}" StrokeThickness="2" />
