                            <mat:Shadow Grid.ColumnSpan="2" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                            <Border
                                    Grid.ColumnSpan="2"
                                    BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    Opacity="0.75"/>
                            <ContentPresenter x:Name="DropImpossibleIconElement"
