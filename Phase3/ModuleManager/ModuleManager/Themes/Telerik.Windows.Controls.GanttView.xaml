<ResourceDictionary
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation"
        xmlns:c="http://schemas.telerik.com/2008/xaml/compile"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="c"
        xmlns:scheduling="clr-namespace:Telerik.Windows.Controls.Scheduling;assembly=Telerik.Windows.Scheduling.Core"
        xmlns:gantt="clr-namespace:Telerik.Windows.Controls.GanttView;assembly=Telerik.Windows.Controls.GanttView"
        xmlns:gantt1="clr-namespace:Telerik.Windows.Controls;assembly=Telerik.Windows.Controls.GanttView"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:mat="clr-namespace:Telerik.Windows.Controls.MaterialControls;assembly=Telerik.Windows.Controls">
    <ResourceDictionary.MergedDictionaries>
        <telerik:MaterialResourceDictionary/>
    </ResourceDictionary.MergedDictionaries>
    <FontFamily x:Key="TelerikWebUI">/Telerik.Windows.Controls;component/Themes/Fonts/TelerikWebUI.ttf#TelerikWebUI</FontFamily>
    <sys:String x:Key="GlyphArrow60Right"></sys:String>
    <sys:String x:Key="GlyphArrow60Down"></sys:String>
    <sys:String x:Key="GlyphArrow60Left"></sys:String>
    <sys:String x:Key="GlyphArrowsKpi"></sys:String>
    <sys:String x:Key="GlyphArrowDown"></sys:String>
    <sys:String x:Key="GlyphLinkHorizontal"></sys:String>
    <sys:String x:Key="GlyphCancel"></sys:String>
    <sys:String x:Key="GlyphPlus"></sys:String>
    <sys:String x:Key="GlyphInsertUp"></sys:String>
    <sys:String x:Key="GlyphInsertMiddle"></sys:String>
    <sys:String x:Key="GlyphInsertDown"></sys:String>
    <telerik:UppercaseConverter x:Key="UpperCaseStringConverter"/>
    <telerik:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
    <telerik:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <gantt1:DateTimeToCurrentUICultureStringConverter x:Key="DateTimeToCurrentUICultureStringConverter"/>
    <telerik:DragDropBooleanToVisibilityConverter x:Key="DragDropBooleanToVisibilityConverter"/>
    <SolidColorBrush x:Key="SemiTransparentBrush" Color="#01FFFFFF"/>
    <Style x:Key="BorderContainerStyle" TargetType="scheduling:BorderContainer">
        <Setter Property="IsHitTestVisible" Value="True"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="5"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 -1 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:BorderContainer">
                    <Border x:Name="Border"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Margin="0"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="AlternatingBorderContainerStyle" TargetType="scheduling:AlternatingBorderContainer" BasedOn="{StaticResource BorderContainerStyle}"/>
    <Style x:Key="SpecialSlotContainerStyle" TargetType="scheduling:SpecialSlotContainer">
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="10"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:SpecialSlotContainer">
                    <Grid>
                        <Border x:Name="Background" Background="{TemplateBinding Background}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Opacity="0.4"/>
                        <Border x:Name="Border"
                                Background="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="GroupTickContainerStyle" TargetType="scheduling:GroupTickContainer">
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="BorderThickness" Value="0 1 1 1"/>
        <Setter Property="Padding" Value="8 2 6 0"/>
        <Setter Property="MinHeight" Value="23"/>
        <Setter Property="VerticalContentAlignment" Value="Top"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:GroupTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch">
                        <TextBlock Text="{Binding FormattedValue}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MajorTickContainerStyle" TargetType="scheduling:MajorTickContainer">
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 1"/>
        <Setter Property="Padding" Value="8 3 0 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:MajorTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch">
                        <TextBlock Text="{Binding FormattedValue, Converter={StaticResource UpperCaseStringConverter}}" Margin="{TemplateBinding Padding}"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MinorTickContainerStyle" TargetType="scheduling:MinorTickContainer">
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="BorderThickness" Value="0 0 1 1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:MinorTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="CurrentHourTickContainerStyle" TargetType="scheduling:CurrentHourTickContainer">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="15"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:CurrentHourTickContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            VerticalAlignment="Stretch"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RowHighlightContainerStyle" TargetType="scheduling:RowHighlightContainer">
        <Setter Property="IsHitTestVisible" Value="False"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="15"/>
        <Setter Property="Padding" Value="0 0 0 1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:RowHighlightContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Background="{TemplateBinding Background}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Margin="{TemplateBinding Padding}"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RowSelectionContainerStyle" TargetType="scheduling:RowSelectionContainer">
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=ComplementaryBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="telerik:ZIndexManager.ZIndex" Value="20"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="scheduling:RowSelectionContainer">
                    <Border
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Margin="{TemplateBinding Padding}">
                        <Border Background="{TemplateBinding Background}" Margin="1 0 1 1"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="scheduling:BorderContainer" BasedOn="{StaticResource BorderContainerStyle}"/>
    <Style TargetType="scheduling:AlternatingBorderContainer" BasedOn="{StaticResource AlternatingBorderContainerStyle}"/>
    <Style TargetType="scheduling:SpecialSlotContainer" BasedOn="{StaticResource SpecialSlotContainerStyle}"/>
    <Style TargetType="scheduling:GroupTickContainer" BasedOn="{StaticResource GroupTickContainerStyle}"/>
    <Style TargetType="scheduling:MajorTickContainer" BasedOn="{StaticResource MajorTickContainerStyle}"/>
    <Style TargetType="scheduling:MinorTickContainer" BasedOn="{StaticResource MinorTickContainerStyle}"/>
    <Style TargetType="scheduling:CurrentHourTickContainer" BasedOn="{StaticResource CurrentHourTickContainerStyle}"/>
    <Style TargetType="scheduling:RowHighlightContainer" BasedOn="{StaticResource RowHighlightContainerStyle}"/>
    <Style TargetType="scheduling:RowSelectionContainer" BasedOn="{StaticResource RowSelectionContainerStyle}"/>
    <DataTemplate x:Key="GanttDragResizeVisualCueTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="140"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <TextBlock Text="{Binding TargetItem.Title}" Grid.ColumnSpan="2" Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}" FontWeight="Medium"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="Start" Grid.Row="1" Margin="0 0 4 0"/>
            <TextBlock
                    Text="{Binding DestinationSlot.Start, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}"
                    Grid.Row="1"
                    Grid.Column="1"
                    Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
            <TextBlock telerik:LocalizationManager.ResourceKey="End" Grid.Row="2" Margin="0 0 4 0"/>
            <TextBlock
                    Text="{Binding DestinationSlot.End, Converter={StaticResource DateTimeToCurrentUICultureStringConverter}}"
                    Grid.Row="2"
                    Grid.Column="1"
                    Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttCreateDependencyTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationFrom"/>
                <TextBlock Text="{Binding DestinationSlot.Source.Title}" Foreground="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}" FontWeight="Medium"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal">
                <TextBlock telerik:LocalizationManager.ResourceKey="DrawRelationTo"/>
                <TextBlock Text="{Binding DestinationSlot.Target.Title}" Foreground="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}" FontWeight="Medium"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="GanttDragReorderTemplate">
        <Grid Margin="{Binding Margin, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=gantt:GanttDragResizeVisualCue}}">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Left">
                <TextBlock telerik:LocalizationManager.ResourceKey="DraggingItem"/>
                <TextBlock Text="{Binding TargetItem.Title}" FontWeight="Medium" Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
            </StackPanel>
            <StackPanel Grid.Row="1" Orientation="Horizontal" HorizontalAlignment="Left">
                <TextBlock
                        HorizontalAlignment="Left"
                        TextWrapping="Wrap"
                        Text="{StaticResource GlyphInsertUp}"
                        VerticalAlignment="Top"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        Foreground="{telerik:MaterialResource ResourceKey=IconBrush}"
                        Margin="0 0 5 0"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Before}"/>
                <TextBlock
                        HorizontalAlignment="Left"
                        TextWrapping="Wrap"
                        Text="{StaticResource GlyphInsertDown}"
                        VerticalAlignment="Top"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        Foreground="{telerik:MaterialResource ResourceKey=IconBrush}"
                        Margin="0 0 5 0"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=After}"/>
                <TextBlock
                        HorizontalAlignment="Left"
                        TextWrapping="Wrap"
                        Text="{StaticResource GlyphInsertMiddle}"
                        VerticalAlignment="Top"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        Foreground="{telerik:MaterialResource ResourceKey=IconBrush}"
                        Margin="0 0 5 0"
                        Visibility="{Binding DestinationSlot.DropPosition, Converter={StaticResource DragDropBooleanToVisibilityConverter}, ConverterParameter=Inside}"/>
                <TextBlock Text="{Binding DestinationSlot.DropPosition}"/>
                <TextBlock Text=":" Margin="0 0 4 0"/>
                <TextBlock Text="{Binding DestinationSlot.GroupKey.Title}" FontWeight="Medium" Foreground="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <DataTemplate x:Key="DragRelationTooltipTemplate">
        <StackPanel Orientation="Horizontal">
            <TextBlock telerik:LocalizationManager.ResourceKey="RelationToolTip" TextWrapping="NoWrap" TextTrimming="None"/>
        </StackPanel>
    </DataTemplate>
    <ControlTemplate x:Key="GanttPresenterSplitterTemplate" TargetType="telerik:ResizeControl">
        <Grid x:Name="Root" IsHitTestVisible="{TemplateBinding IsEnabled}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver"/>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualState x:Name="Unfocused"/>
                    <VisualState x:Name="Focused">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="FocusedVisual" Storyboard.TargetProperty="Opacity" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border
                    Width="{TemplateBinding Width}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}"
                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"/>
            <Rectangle x:Name="FocusedVisual" Fill="Transparent" Margin="0" Opacity="0"/>
            <Rectangle x:Name="PART_PreviewControl"
                    VerticalAlignment="Stretch"
                    HorizontalAlignment="Stretch"
                    Fill="{telerik:MaterialResource ResourceKey=PrimaryFocusBrush}"
                    Opacity="0.50"
                    Visibility="Collapsed"
                    Width="9"/>
        </Grid>
    </ControlTemplate>
    <Style x:Key="GanttPresenterSplitterStyle" TargetType="telerik:ResizeControl">
        <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerSolidBrush}"/>
        <Setter Property="BorderThickness" Value="1 0"/>
        <Setter Property="Width" Value="11"/>
        <Setter Property="Margin" Value="0 0 -1 0"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
        <Setter Property="ShowsPreview" Value="True"/>
        <Setter Property="Template" Value="{StaticResource GanttPresenterSplitterTemplate}"/>
    </Style>
    <Style x:Key="GanttPresenterHeaderSplitterStyle" TargetType="telerik:ResizeControl" BasedOn="{StaticResource GanttPresenterSplitterStyle}">
        <Setter Property="Width" Value="6"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="BorderThickness" Value="0 0 1 0"/>
        <Setter Property="Background" Value="{StaticResource SemiTransparentBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="scheduling:ZIndexManager.ZIndex" Value="100"/>
    </Style>
    <ControlTemplate x:Key="GanttItemsPresenterTemplate" TargetType="gantt:GanttItemsPresenter">
        <telerik:GanttPresenterPanel>
            <telerik:GanttPresenterPanel.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*" MinWidth="3"/>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="Auto"/>
            </telerik:GanttPresenterPanel.ColumnDefinitions>
            <telerik:TreeRenderingPanel x:Name="ColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Header"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Stretch"
                    Margin="-1 0 4 0"/>
            <telerik:VirtualizedGridPanel x:Name="CellsPanel"
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="Content"
                    Margin="-1 0 4 0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    VerticalOffset="{Binding VerticalOffset, ElementName=FrozenCellsPanel, Mode=TwoWay}"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"/>
            <telerik:TreeRenderingPanel x:Name="FrozenColumnsPanel"
                    Orientation="Horizontal"
                    telerik:GanttPresenterPanel.Column="0"
                    telerik:GanttPresenterPanel.Area="Header"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Stretch"
                    Margin="1 0 0 0"/>
            <telerik:VirtualizedGridPanel x:Name="FrozenCellsPanel"
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Top"
                    telerik:GanttPresenterPanel.Column="0"
                    telerik:GanttPresenterPanel.Area="Content"
                    Margin="1 0 0 0"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="1"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=ColumnsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=ColumnsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    SmallChange="40"
                    LargeChange="{Binding ViewportWidth, ElementName=ColumnsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=ColumnsPanel}"
                    Margin="0 -1 9 0"/>
            <telerik:ResizeControl telerik:GanttPresenterPanel.Column="1" telerik:GanttPresenterPanel.Area="Splitter" Style="{StaticResource GanttPresenterSplitterStyle}"/>
            <telerik:LogicalCanvasPanel x:Name="TimeRulerPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Header"
                    VerticalAlignment="Bottom"
                    HorizontalAlignment="Left"
                    HorizontalOffset="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"/>
            <telerik:LogicalCanvasPanel x:Name="EventsPanel"
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="Content"
                    VerticalOffset="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    Background="Transparent"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Left"
                    Margin="-1 0 0 0"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="2"
                    telerik:GanttPresenterPanel.Area="ScrollBar"
                    Orientation="Horizontal"
                    Minimum="0"
                    Maximum="{Binding ExtentWidth, ElementName=EventsPanel}"
                    Value="{Binding HorizontalOffset, ElementName=EventsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportWidth, ElementName=EventsPanel}"
                    SmallChange="20"
                    LargeChange="{Binding ViewportWidth, ElementName=EventsPanel}"
                    Visibility="{Binding CalculatedHorizontalScrollBarVisibility, ElementName=EventsPanel}"
                    Margin="-1 -1 0 0"/>
            <ScrollBar
                    telerik:GanttPresenterPanel.Column="3"
                    telerik:GanttPresenterPanel.Area="Content"
                    Orientation="Vertical"
                    Minimum="0"
                    Maximum="{Binding ExtentHeight, ElementName=CellsPanel}"
                    Value="{Binding VerticalOffset, ElementName=CellsPanel, Mode=TwoWay}"
                    ViewportSize="{Binding ViewportHeight, ElementName=CellsPanel}"
                    Visibility="{Binding CalculatedVerticalScrollBarVisibility, ElementName=CellsPanel}"
                    SmallChange="10"
                    Margin="-1 0 0 0"
                    LargeChange="{Binding ViewportHeight, ElementName=CellsPanel}"/>
        </telerik:GanttPresenterPanel>
    </ControlTemplate>
    <Style x:Key="GanttItemsPresenterStyle" TargetType="gantt:GanttItemsPresenter">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template" Value="{StaticResource GanttItemsPresenterTemplate}"/>
    </Style>
    <ControlTemplate x:Key="GanttDragResizeVisualCueControlTemplate" TargetType="gantt:GanttDragResizeVisualCue">
        <Grid Margin="5">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="EffectStates">
                    <VisualState x:Name="None">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="NoneEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Move"/>
                    <VisualState x:Name="Copy">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="CopyEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Link">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LinkEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Scroll">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ScrollEffect" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}" Opacity="0.5"/>
                <Border
                        Padding="{TemplateBinding BorderThickness}"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        Opacity="0.5"/>
                <Grid Margin="{TemplateBinding Padding}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock x:Name="NoneEffect"
                            Visibility="Collapsed"
                            Text="{StaticResource GlyphCancel}"
                            VerticalAlignment="Top"
                            HorizontalAlignment="Left"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontSize="16"
                            FontStyle="Normal"
                            FontWeight="Normal"
                            Foreground="{telerik:MaterialResource ResourceKey=ValidationBrush}"
                            Margin="0 1 3 1"/>
                    <TextBlock x:Name="CopyEffect"
                            Visibility="Collapsed"
                            Text="{StaticResource GlyphPlus}"
                            VerticalAlignment="Top"
                            HorizontalAlignment="Left"
                            FontFamily="{StaticResource TelerikWebUI}"
                            FontSize="16"
                            FontStyle="Normal"
                            FontWeight="Normal"
                            Foreground="{telerik:MaterialResource ResourceKey=IconBrush}"
