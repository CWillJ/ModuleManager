                            mat:MaterialAssist.PressedBrush="{x:Null}"
                            Clip="{Binding ElementName=clipEllipse, Path=RenderedGeometry}"
                            ClipToBounds="True"
                            IsRippleCentered="True"
                            RenderTransformOrigin=".5 .5"
                            RippleBrush="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}">
                            <mat:MaterialControl.RenderTransform>
                                <ScaleTransform ScaleX="2" ScaleY="2" />
                            </mat:MaterialControl.RenderTransform>
                        </mat:MaterialControl>
                        <Ellipse
                            x:Name="grip"
                            Width="12"
                            Height="12"
                            Fill="{TemplateBinding Background}"
                            IsHitTestVisible="False"
                            RenderTransformOrigin=".5,.5"
                            Stroke="{TemplateBinding BorderBrush}"
                            StrokeThickness="{TemplateBinding BorderThickness}">
