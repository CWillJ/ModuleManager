        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="Padding" Value="4,0" />
        <Setter Property="ButtonPartStyle" Value="{StaticResource BreadCrumbButtonStyle}" />
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbToggleButtonStyle}" />
        <Setter Property="DropDownIndicatorVisibility" Value="Collapsed" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid SnapsToDevicePixels="True">
                        <mat:Shadow
                            x:Name="Shadow"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{TemplateBinding CornerRadius}" />
                        <Border
                            x:Name="BorderVisual"
                            Background="{x:Null}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{TemplateBinding CornerRadius}"
                            IsHitTestVisible="False"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <mat:MaterialControl
                            CornerRadius="{TemplateBinding CornerRadius}"
                            IsRippleEnabled="False"
                            IsSmartClipped="True">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <telerik:RadButton
                                    x:Name="ButtonPart"
                                    Grid.Row="1"
                                    Grid.Column="1"
                                    Content="{TemplateBinding Content}"
                                    ContentStringFormat="{TemplateBinding ContentStringFormat}"
                                    ContentTemplate="{TemplateBinding ContentTemplate}"
                                    ContentTemplateSelector="{TemplateBinding ContentTemplateSelector}"
                                    IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                    Style="{TemplateBinding ButtonPartStyle}" />
                                <telerik:RadToggleButton
                                    x:Name="DropDownPart"
                                    Grid.Row="1"
                                    Grid.Column="2"
                                    IsBackgroundVisible="{TemplateBinding IsBackgroundVisible}"
                                    IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                    Style="{TemplateBinding TogglePartStyle}"
                                    Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                    <telerik:RadToggleButton.Content>
                                        <TextBlock
                                            x:Name="arrow"
                                            telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                            FontFamily="{StaticResource TelerikWebUI}"
                                            FontSize="16"
                                            FontStyle="Normal"
                                            FontWeight="Normal"
                                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik:RadToggleButton}}}"
                                            IsHitTestVisible="False"
                                            Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}">
                                            <Run x:Name="arrowRun" Text="{StaticResource GlyphArrowChevronRight}" />
                                        </TextBlock>
                                    </telerik:RadToggleButton.Content>
                                </telerik:RadToggleButton>
                            </Grid>
                        </mat:MaterialControl>
                        <Popup
                            x:Name="DropDownPopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            HorizontalOffset="0"
                            Placement="Bottom"
                            PopupAnimation="{TemplateBinding PopupAnimation}"
                            StaysOpen="False"
                            VerticalOffset="0">
                            <Grid x:Name="PopupGrid" Margin="8,0,8,8">
                                <mat:Shadow
                                    x:Name="PopupShadow"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    BorderBrush="{TemplateBinding BorderBrush}"
                                    BorderThickness="{TemplateBinding BorderThickness}"
                                    CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                    ShadowDepth="Depth2"
                                    SnapsToDevicePixels="True" />
                                <Border
                                    x:Name="DropDownPopupBorder"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MinWidth="3"
                                    MinHeight="3"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                    BorderThickness="0,1,0,0"
                                    CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}">
                                    <ContentControl
                                        x:Name="DropDownPopupContent"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        HorizontalContentAlignment="Stretch"
                                        VerticalContentAlignment="Stretch"
                                        Content="{TemplateBinding DropDownContent}"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsOpen" Value="False" />
                                <Condition SourceName="DropDownPart" Property="IsPressed" Value="False" />
                                <Condition SourceName="ButtonPart" Property="IsPressed" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter Property="Background" Value="Transparent" />
                        </MultiTrigger>

                        <!--  Is any of the parts pressed  -->
                        <Trigger SourceName="DropDownPart" Property="IsPressed" Value="True">
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
                        </Trigger>
                        <Trigger SourceName="ButtonPart" Property="IsPressed" Value="True">
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
                        </Trigger>

                        <!--  disabled  -->
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="DropDownPart" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
                        </Trigger>

                        <!--  enabled & opened  -->
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsEnabled" Value="True" />
                                <Condition Property="IsOpen" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
                            <Setter TargetName="DropDownPart" Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=IconBrush}" />
                            <Setter TargetName="arrowRun" Property="Text" Value="{StaticResource GlyphArrowChevronDown}" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style
        x:Key="BreadcrumbOverflowSplitButtonStyle"
        BasedOn="{StaticResource RadSplitButtonStyle}"
        TargetType="telerik:RadSplitButton">
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="mat:MaterialAssist.IsShadowDisabled" Value="True" />
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="{x:Null}" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="IsOpen" Value="False" />
        <Setter Property="IsButtonPartVisible" Value="False" />
        <Setter Property="Width" Value="Auto" />
        <Setter Property="MinWidth" Value="36" />
        <Setter Property="MaxWidth" Value="150" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="BorderBrush" Value="{x:Null}" />
        <Setter Property="DropDownMaxHeight" Value="700" />
        <Setter Property="DropDownMaxWidth" Value="400" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="8,0" />
    </Style>
    <Style
        x:Key="BreadcrumbTextSplitButton"
        BasedOn="{StaticResource RadSplitButtonStyle}"
        TargetType="telerik:RadSplitButton">
        <Setter Property="CornerRadius" Value="0" />
        <Setter Property="TogglePartStyle" Value="{StaticResource BreadCrumbHistoryToggleButtonStyle}" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="Transparent" />
        <Setter Property="Padding" Value="3" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="IsOpen" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadSplitButton">
                    <Grid>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <ContentControl
                                x:Name="Content"
                                HorizontalContentAlignment="Stretch"
                                VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                Content="{TemplateBinding Content}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Foreground="{TemplateBinding Foreground}"
                                IsTabStop="False" />
                            <telerik:RadToggleButton
                                x:Name="DropDownPart"
                                Grid.Column="1"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                Focusable="False"
                                Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                                IsChecked="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                                IsTabStop="False"
                                Style="{TemplateBinding TogglePartStyle}"
                                Visibility="{TemplateBinding DropDownIndicatorVisibility}">
                                <TextBlock
                                    x:Name="DropDownIndicator"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                    FontFamily="{StaticResource TelerikWebUI}"
                                    FontSize="{StaticResource BreadCrumbGlyphSize}"
                                    FontStyle="Normal"
                                    FontWeight="Normal"
                                    IsHitTestVisible="False"
                                    Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}">
                                    <Run Text="{StaticResource GlyphArrowOverflowDown}" />
                                </TextBlock>
                            </telerik:RadToggleButton>
                        </Grid>
                        <Popup
                            x:Name="DropDownPopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            IsOpen="{Binding Path=IsOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                            Placement="Bottom"
                            StaysOpen="False">
                            <Grid Margin="8,0,8,8">
                                <mat:Shadow
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                    ShadowDepth="Depth2" />
                                <Border
                                    x:Name="DropDownPopupBorder"
                                    Width="{TemplateBinding DropDownWidth}"
                                    Height="{TemplateBinding DropDownHeight}"
                                    MaxWidth="{TemplateBinding DropDownMaxWidth}"
                                    MaxHeight="{TemplateBinding DropDownMaxHeight}"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                    BorderThickness="0,1,0,0"
                                    CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}">
                                    <ContentPresenter
                                        x:Name="DropDownPopupContent"
                                        HorizontalAlignment="Stretch"
                                        VerticalAlignment="Stretch"
                                        Content="{TemplateBinding DropDownContent}"
                                        ContentTemplate="{TemplateBinding DropDownContentTemplate}"
                                        DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=DataContext}" />
                                </Border>
                            </Grid>
                        </Popup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <ControlTemplate x:Key="BarItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <Border
                x:Name="FocusVisual"
                Background="{TemplateBinding mat:MaterialAssist.FocusBrush}"
                BorderBrush="{x:Null}"
                BorderThickness="{TemplateBinding BorderThickness}"
                IsHitTestVisible="False"
                Visibility="Collapsed" />
            <Border
                x:Name="headerContainer"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}">
                <telerik:RadSplitButton
                    x:Name="SplitButton"
                    Margin="0"
                    Command="{x:Static telerikBreadcrumb:RadBreadcrumbCommands.ClearSelectedItem}"
                    Content="{TemplateBinding Header}"
                    ContentTemplate="{TemplateBinding HeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{Binding Foreground, RelativeSource={RelativeSource TemplatedParent}}"
                    IsOpen="{Binding Path=IsPopupOpen, Mode=TwoWay, RelativeSource={RelativeSource TemplatedParent}}"
                    Style="{TemplateBinding SplitButtonStyle}">
                    <telerik:RadSplitButton.DropDownContent>
                        <ScrollViewer
                            MinWidth="148"
                            MaxWidth="270"
                            MaxHeight="250"
                            Margin="0"
                            Padding="0"
                            BorderThickness="0"
                            HorizontalScrollBarVisibility="Auto"
                            VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter />
                        </ScrollViewer>
                    </telerik:RadSplitButton.DropDownContent>
                </telerik:RadSplitButton>
            </Border>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="SplitButtonVisibility">
                    <VisualState x:Name="SplitButtonVisible" />
                    <VisualState x:Name="SplitButtonHidden">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="SplitButton"
                                Storyboard.TargetProperty="Visibility"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="HasItems" Value="False">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="HasItems" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsLinearMode" Value="True">
                <Setter TargetName="SplitButton" Property="DropDownIndicatorVisibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsButtonPartHidden" Value="True">
                <Setter TargetName="SplitButton" Property="IsButtonPartVisible" Value="False" />
            </Trigger>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="FocusVisual" Property="Visibility" Value="Collapsed" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <ControlTemplate x:Key="DropDownItemControlTemplate" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Grid>
            <Border
                x:Name="BorderVisual"
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}" />
            <Grid Background="Transparent">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="29" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Image
                    Margin="-3,0,0,0"
                    Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Image}"
                    Style="{StaticResource BreadcrumbImageStyle}"
                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                <ContentControl
                    x:Name="contentControl"
                    Grid.Column="1"
                    Margin="{TemplateBinding Padding}"
                    VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                    Content="{TemplateBinding DropDownHeader}"
                    ContentTemplate="{TemplateBinding DropDownHeaderTemplate}"
                    ContentTemplateSelector="{TemplateBinding HeaderTemplateSelector}"
                    FontSize="{TemplateBinding FontSize}"
                    Foreground="{TemplateBinding Foreground}" />
            </Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="Mouse">
                    <VisualState x:Name="MouseEnter">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="BorderVisual"
                                Storyboard.TargetProperty="Background"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:MaterialResource ResourceKey=AlternativeBrush}" />
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseLeave" />
                </VisualStateGroup>
                <VisualStateGroup x:Name="Selection">
                    <VisualState x:Name="Select">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="contentControl"
                                Storyboard.TargetProperty="FontWeight"
                                Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <FontWeight>Bold</FontWeight>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames
                                Storyboard.TargetName="BorderVisual"
                                Storyboard.TargetProperty="Background"
                                Duration="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="UnSelect" />
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
        </Grid>
    </ControlTemplate>
    <Style x:Key="RadBreadcrumbStyle" TargetType="telerikNavigation:RadBreadcrumb">
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="MinWidth" Value="150" />
        <Setter Property="SplitButtonStyle" Value="{StaticResource BreadcrumbSplitButtonStyle}" />
        <Setter Property="OverflowButtonStyle" Value="{StaticResource BreadcrumbOverflowSplitButtonStyle}" />
        <Setter Property="DropDownItemControlTemplate" Value="{StaticResource DropDownItemControlTemplate}" />
        <Setter Property="BarItemControlTemplate" Value="{StaticResource BarItemControlTemplate}" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadBreadcrumb">
                    <Grid x:Name="outerGrid">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="Auto" />
                            <ColumnDefinition Width="*" />
                            <ColumnDefinition Width="36" />
                        </Grid.ColumnDefinitions>
                        <mat:Shadow
                            Grid.ColumnSpan="3"
                            Background="{TemplateBinding Background}"
                            ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                        <Border
                            x:Name="NormalVisual"
                            Grid.ColumnSpan="3"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                        <Image
                            x:Name="image"
                            Margin="12,0"
                            Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=CurrentContainer.Image}"
                            Style="{StaticResource BreadcrumbImageStyle}"
                            Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsCurrentIconVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                        <telerik:RadSplitButton
                            x:Name="historySplitButton"
                            Grid.Column="1"
                            Grid.ColumnSpan="2"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            IsOpen="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHistoryOpen, Mode=TwoWay}"
                            IsTabStop="False"
                            Style="{StaticResource BreadcrumbTextSplitButton}"
                            TogglePartStyle="{StaticResource BreadCrumbHistoryToggleButtonStyle}">
                            <telerik:RadSplitButton.Content>
                                <TextBox
                                    x:Name="pathTextBox"
                                    MinHeight="20"
                                    VerticalAlignment="Center"
                                    mat:MaterialAssist.FocusBrush="{x:Null}"
                                    BorderThickness="0"
                                    FontSize="{TemplateBinding FontSize}"
                                    IsTabStop="False"
                                    Opacity="0" />
                            </telerik:RadSplitButton.Content>
                            <telerik:RadSplitButton.DropDownContent>
                                <ListBox
                                    x:Name="HistoryListBox"
                                    MinHeight="36"
                                    HorizontalContentAlignment="Stretch"
                                    VerticalContentAlignment="Stretch"
                                    mat:MaterialAssist.CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                    mat:MaterialAssist.ShadowDepth="Depth0"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    BorderThickness="0"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{TemplateBinding Foreground}"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    ItemsSource="{TemplateBinding HistoryItems}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedHistoryItem, Mode=TwoWay}" />
                            </telerik:RadSplitButton.DropDownContent>
                        </telerik:RadSplitButton>
                        <telerikBreadcrumb:RadBreadcrumbBar
                            x:Name="BreadcrumbBar"
                            Grid.Column="1"
                            Background="Transparent"
                            FontSize="{TemplateBinding FontSize}"
                            Foreground="{TemplateBinding Foreground}"
                            ItemTemplate="{TemplateBinding ItemTemplate}"
                            ItemTemplateSelector="{TemplateBinding ItemTemplateSelector}" />
                        <Border
                            x:Name="FocusVisual"
                            Grid.ColumnSpan="3"
                            Background="{x:Null}"
                            BorderBrush="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            Visibility="Collapsed" />
                        <Popup
                            x:Name="AutocompletePopup"
                            AllowsTransparency="True"
                            Focusable="False"
                            Placement="Bottom"
                            StaysOpen="False">
                            <Grid Margin="8,0,8,8">
                                <ListBox
                                    x:Name="AutoCompleteListBox"
                                    MinWidth="150"
                                    MinHeight="36"
                                    MaxWidth="650"
                                    MaxHeight="500"
                                    mat:MaterialAssist.CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
                                    mat:MaterialAssist.ShadowDepth="Depth2"
                                    Background="{telerik1:MaterialResource ResourceKey=MainBrush}"
                                    BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}"
                                    BorderThickness="0,1,0,0"
                                    FontSize="{TemplateBinding FontSize}"
                                    Foreground="{TemplateBinding Foreground}"
                                    ItemContainerStyle="{StaticResource BreadcrumbListBoxItemStyle}"
                                    ItemsSource="{TemplateBinding AutocompleteItems}"
                                    SelectedItem="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SelectedAutocompleteItem, Mode=TwoWay}" />
                            </Grid>
                        </Popup>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                        </Trigger>
                        <Trigger Property="IsTextModeEnabled" Value="False">
                            <Setter TargetName="historySplitButton" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Collapsed" />
                            <Setter TargetName="FocusVisual" Property="Visibility" Value="Visible" />
                            <Setter TargetName="pathTextBox" Property="Opacity" Value="1" />
                            <Setter TargetName="NormalVisual" Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsTextModeEnabled" Value="True" />
                                <Condition Property="IsTextMode" Value="False" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BreadcrumbBar" Property="Visibility" Value="Visible" />
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBar">
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal" />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikBreadcrumb:RadBreadcrumbBar">
                    <Grid>
                        <Border
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <ItemsPresenter x:Name="ItemsPresented" ClipToBounds="True" />
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadBreadcrumbBarItemStyle" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem">
        <Setter Property="mat:MaterialAssist.FocusBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="FocusVisualStyle" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="Padding" Value="8,0" />
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
    </Style>
    <Style BasedOn="{StaticResource RadBreadcrumbStyle}" TargetType="telerikNavigation:RadBreadcrumb" />
    <Style BasedOn="{StaticResource RadBreadcrumbBarStyle}" TargetType="telerikBreadcrumb:RadBreadcrumbBar" />
    <Style BasedOn="{StaticResource RadBreadcrumbBarItemStyle}" TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" />
    <Style
        x:Key="DesktopAlertCloseButtonStyle"
        BasedOn="{StaticResource RadButtonStyle}"
        TargetType="telerik:RadButton">
        <Setter Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}" />
        <Setter Property="mat:MaterialAssist.PressedBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}" />
        <Setter Property="Padding" Value="10,10,9,10" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="MinWidth" Value="36" />
        <Setter Property="VerticalAlignment" Value="Center" />
