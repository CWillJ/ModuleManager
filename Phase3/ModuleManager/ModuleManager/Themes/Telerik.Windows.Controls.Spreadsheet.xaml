                                <ListBox.ItemsPanel>
                                    <ItemsPanelTemplate>
                                        <telerik:RadWrapPanel Orientation="Horizontal" MaxWidth="{Binding QuickStylesWidth}"/>
                                    </ItemsPanelTemplate>
                                </ListBox.ItemsPanel>
                            </ListBox>
                            <Grid Grid.Column="1" Margin="0" Width="36">
