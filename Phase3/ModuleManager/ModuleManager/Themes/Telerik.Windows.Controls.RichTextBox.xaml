    <ControlTemplate x:Key="ApplicationRibbonDropDownButtonTemplate" TargetType="telerikRibbonView:RadRibbonDropDownButton">
        <Grid SnapsToDevicePixels="True">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="DropDownContentStates">
                    <VisualState x:Name="HasDropDownContent"/>
                    <VisualState x:Name="WithoutDropDownContent">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Visibility" Storyboard.TargetName="DropDownPanel">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="TextAlignment">
                    <VisualState x:Name="TextHide">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="textRow1" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="BorderVisual" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}"/>
            <Border x:Name="CheckVisual"
                    Opacity="0"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    Background="{TemplateBinding mat:MaterialAssist.CheckedBrush}"/>
            <mat:MaterialControl CornerRadius="{TemplateBinding CornerRadius}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" IsSmartClipped="True">
                <Grid Margin="{TemplateBinding Padding}">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ContentControl
                            Grid.ColumnSpan="3"
                            x:Name="ContentPresenter"
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            Content="{TemplateBinding Content}"
                            ContentTemplate="{TemplateBinding ContentTemplate}"/>
                    <Image x:Name="Image" Margin="0 0 24 0"/>
                    <TextBlock x:Name="textRow1" Grid.Column="1" Text="{TemplateBinding Text}" TextAlignment="Left" VerticalAlignment="Center" Margin="{TemplateBinding Padding}"/>
                    <Grid x:Name="DropDownPanel" Grid.Column="2" Margin="16 0 0 0" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <TextBlock x:Name="DropDownIndicator"
                                telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                                Visibility="{TemplateBinding DropDownIndicatorVisibility}"
                                FontSize="16"
                                Foreground="{telerikControls:MaterialResource ResourceKey=IconBrush}"
                                Opacity="{telerikControls:MaterialResource ResourceKey=SecondaryOpacity}"
                                FontFamily="{StaticResource TelerikWebUI}"
                                FontWeight="Normal"
                                FontStyle="Normal">
                            <Run x:Name="DropDownIndicatorRun" Text="{StaticResource GlyphArrow60Right}"/>
                        </TextBlock>
                    </Grid>
                </Grid>
            </mat:MaterialControl>
            <Popup x:Name="DropDownPopup"
                    AllowsTransparency="True"
                    StaysOpen="False"
                    Focusable="False"
                    Placement="Bottom"
                    IsOpen="{Binding Path=IsOpen, RelativeSource={RelativeSource TemplatedParent}}">
                <Grid>
                    <Border x:Name="DropDownPopupBorder"
                            Background="{telerikControls:MaterialResource ResourceKey=MainBrush}"
                            Width="{TemplateBinding DropDownWidth}"
                            Height="{TemplateBinding DropDownHeight}">
                        <ContentControl x:Name="DropDownPopupContent" Content="{TemplateBinding DropDownContent}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Image" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="ContentPresenter" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsOpen" Value="True">
                <Setter TargetName="DropDownIndicatorRun" Property="Text" Value="{StaticResource GlyphArrow60Left}"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="True"/>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="Background" TargetName="BorderVisual" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(mat:MaterialAssist.CheckedBrush), Mode=OneWay}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsOpen" Value="True"/>
                    <Condition Property="animation:AnimationManager.IsAnimationEnabled" Value="True"/>
                </MultiTrigger.Conditions>
                <MultiTrigger.EnterActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="0" To="1" Storyboard.TargetName="CheckVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.EnterActions>
                <MultiTrigger.ExitActions>
                    <BeginStoryboard>
                        <Storyboard>
                            <DoubleAnimation Duration="00:00:00.20" From="1" To="0" Storyboard.TargetName="CheckVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </BeginStoryboard>
                </MultiTrigger.ExitActions>
            </MultiTrigger>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter TargetName="DropDownIndicator" Property="Text" Value="{StaticResource GlyphArrow60Left}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
