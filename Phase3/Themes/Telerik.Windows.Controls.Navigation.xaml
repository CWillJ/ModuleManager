                                            FontStyle="Normal"
                                            FontWeight="Normal"
                                            Foreground="{Binding Foreground, RelativeSource={RelativeSource AncestorType={x:Type telerik1:RadToggleButton}}}"
                                            Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}"
                                            Visibility="Visible">
                                            <Run x:Name="arrowRun" Text="{StaticResource GlyphArrow60Down}" />
                                        </TextBlock>
