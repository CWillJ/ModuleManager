                    Background="{telerikControls:MaterialResource ResourceKey=AlternativeBrush}"
                    BorderBrush="{telerikControls:MaterialResource ResourceKey=AlternativeBrush}"/>
            <mat:MaterialControl CornerRadius="{TemplateBinding CornerRadius}" IsSmartClipped="True">
                <TextBlock x:Name="MinimizeButtonContent"
