    <Style x:Key="AdornerControlStyle" TargetType="spreadsheetControls:AdornerControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid x:Name="AdornerRoot">
                        <Grid x:Name="gridControl" RenderTransformOrigin="0.5,0.5" ShowGridLines="False" Background="Transparent">
                            <Grid.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHorizontallyFlipped, Converter={StaticResource BoolToIntegerConverter}}" ScaleY="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsVerticallyFlipped, Converter={StaticResource BoolToIntegerConverter}}"/>
                                    <RotateTransform Angle="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=RotationAngle}"/>
                                </TransformGroup>
                            </Grid.RenderTransform>
                            <Grid.Resources>
                                <Style TargetType="Ellipse" x:Key="ResizeStyle">
                                    <Setter Property="Margin" Value="-7"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
                                </Style>
                                <Style TargetType="Rectangle" x:Key="ResizeNoEdgeStyle">
                                    <Setter Property="Margin" Value="-7"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
                                </Style>
                                <Style TargetType="Ellipse" x:Key="RotateStyle">
                                    <Setter Property="Margin" Value="-30"/>
                                    <Setter Property="Width" Value="14"/>
                                    <Setter Property="Height" Value="14"/>
                                    <Setter Property="StrokeThickness" Value="1"/>
                                    <Setter Property="Stroke" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                                    <Setter Property="Fill" Value="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0"/>
                                <RowDefinition/>
                                <RowDefinition Height="0"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="0"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="0"/>
                            </Grid.ColumnDefinitions>
                            <Image x:Name="ImagePreview" Stretch="Fill" Grid.Row="1" Grid.Column="1" Visibility="Collapsed" Opacity="1"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Width="1"
                                    Fill="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    Grid.RowSpan="3"
                                    HorizontalAlignment="Left"
                                    Grid.Column="1"
                                    VerticalAlignment="Stretch"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Width="1"
                                    Fill="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    Grid.RowSpan="3"
                                    HorizontalAlignment="Right"
                                    Grid.Column="1"
                                    VerticalAlignment="Stretch"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Height="1"
                                    Fill="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Stretch"
                                    Grid.Row="1"
                                    VerticalAlignment="Top"/>
                            <Rectangle
                                    StrokeThickness="0"
                                    Height="1"
                                    Fill="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    Stroke="Black"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Stretch"
                                    Grid.Row="1"
                                    VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="rotateLine"
                                    Visibility="{Binding Path=IsRotationEnabled, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    StrokeThickness="0"
                                    Height="24"
                                    Width="1"
                                    Fill="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    Grid.ColumnSpan="3"
                                    HorizontalAlignment="Center"
                                    Grid.Row="1"
                                    VerticalAlignment="Top"
                                    Margin="0 -20 0 0"/>
                            <Ellipse x:Name="Rotate"
                                    Visibility="{Binding Path=IsRotationEnabled, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BoolToVisibilityValueConverter}}"
                                    Style="{StaticResource RotateStyle}"
                                    Stroke="{telerik:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Grid.ColumnSpan="3"
                                    Grid.RowSpan="3"/>
                            <Ellipse x:Name="Resize00" Style="{StaticResource ResizeStyle}" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize10" Style="{StaticResource ResizeStyle}" Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Top"/>
                            <Ellipse x:Name="Resize01" Style="{StaticResource ResizeStyle}" Grid.Row="2" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
                            <Ellipse x:Name="Resize11" Style="{StaticResource ResizeStyle}" Grid.Column="2" Grid.Row="2" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
                            <Rectangle x:Name="ResizeTop" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Top" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeBottom" Style="{StaticResource ResizeNoEdgeStyle}" VerticalAlignment="Bottom" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeLeft" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Left" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                            <Rectangle x:Name="ResizeRight" Style="{StaticResource ResizeNoEdgeStyle}" HorizontalAlignment="Right" Grid.ColumnSpan="3" Grid.RowSpan="3"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FunctionsMenuStyle" TargetType="spreadsheetControls:FunctionsMenu">
        <Setter Property="FontFamily" Value="{telerik:MaterialResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik:MaterialResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:MaterialResource ResourceKey=DividerBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="spreadsheetControls:FunctionsMenu">
                    <telerik:RadContextMenu BorderThickness="0" Padding="0" mat:MaterialAssist.ShadowDepth="Depth0">
                        <telerik:RadMenuGroupItem
                                Background="Transparent"
                                ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Items}"
                                DataContext="{Binding}"
                                IsEnabled="{Binding Path=ApplyFunction.IsEnabled}"
                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                MaxHeight="500"
                                StaysOpenOnClick="False"
                                MinWidth="120">
                            <telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                                <spreadsheetUtilities:SingleTemplateSelector>
                                    <spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                        <DataTemplate>
                                            <telerik:RadMenuItem
                                                    Header="{Binding Name}"
                                                    CommandParameter="{Binding Name}"
                                                    Command="{Binding Path=DataContext.ApplyFunction.Command, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}"
                                                    IsEnabled="{Binding Path=DataContext.ApplyFunction.IsEnabled, RelativeSource={RelativeSource AncestorType=spreadsheetControls:FunctionsMenu}}">
                                                <ToolTipService.ToolTip>
                                                    <ToolTip MinHeight="50">
                                                        <StackPanel>
                                                            <TextBlock Text="{Binding Name}" FontWeight="Medium" FontSize="{telerik:MaterialResource ResourceKey=FontSizeS}"/>
                                                            <TextBlock Text="{Binding Path=FunctionInfo.Description}" TextWrapping="Wrap" MaxWidth="200" Margin="0 8 0 0" FontSize="{telerik:MaterialResource ResourceKey=FontSizeS}"/>
                                                        </StackPanel>
                                                    </ToolTip>
                                                </ToolTipService.ToolTip>
                                            </telerik:RadMenuItem>
                                        </DataTemplate>
                                    </spreadsheetUtilities:SingleTemplateSelector.MenuItemTemplate>
                                </spreadsheetUtilities:SingleTemplateSelector>
                            </telerik:RadMenuGroupItem.ItemContainerTemplateSelector>
                        </telerik:RadMenuGroupItem>
                        <telerik:RadMenuSeparatorItem/>
                        <telerik:RadMenuItem
                                telerik:LocalizationManager.ResourceKey="Spreadsheet_FunctionsMenu_InsertFunction"
                                Command="{Binding Path=ShowInsertFunctionDialog.Command}"
                                IsEnabled="{Binding Path=ShowInsertFunctionDialog.IsEnabled}"
                                CommandParameter="{Binding Path=Category, RelativeSource={RelativeSource TemplatedParent}}">
                            <telerik:RadMenuItem.Icon>
                                <TextBlock FontFamily="{StaticResource TelerikWebUI}" FontSize="16" FontStyle="Normal" FontWeight="Normal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Run Text="{StaticResource GlyphFormulaFx}"/>
                                </TextBlock>
                            </telerik:RadMenuItem.Icon>
                        </telerik:RadMenuItem>
                    </telerik:RadContextMenu>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FilteringPopupControlStyle" TargetType="spreadsheetControls:FilteringPopupControl">
