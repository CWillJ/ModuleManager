                            </Grid>
                            <Border Margin="4 4 0 0" Grid.Row="1" Background="{telerik1:MaterialResource ResourceKey=AlternativeBrush}">
                                <ContentPresenter x:Name="SelectedTabContent"/>
                            </Border>
                            <Grid Grid.Row="2">
                                <mat:Shadow ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{TemplateBinding Background}"/>
                                <Border BorderThickness="0" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}">
                                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                        <telerik:RadButton x:Name="OpenCloseButton" Style="{TemplateBinding OpenCloseButtonStyle}">
                                            <ToolTipService.ToolTip>
                                                <ToolTip Content="{telerik:LocalizableResource Key=Open_Close}"/>
                                            </ToolTipService.ToolTip>
                                        </telerik:RadButton>
                                        <TextBlock
