                                    Grid.Column="1"
                                    ItemsSource="{Binding SelectedChartModel.ChildTypes}"
                                    ItemTemplate="{StaticResource ChartChildTypeItemTemlate}"
                                    HorizontalAlignment="Stretch"
                                    Margin="10"
                                    SelectedItem="{Binding SelectedChartModel.SelectedChildModel, Mode=TwoWay}"
                                    BorderThickness="0"
                                    Background="{x:Null}"
                                    VerticalAlignment="Top"
                                    ScrollViewer.VerticalScrollBarVisibility="Disabled">
