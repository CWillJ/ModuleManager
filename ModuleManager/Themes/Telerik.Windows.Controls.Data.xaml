                        <Thumb
                            x:Name="PART_ResizeThumb"
                            Style="{StaticResource CollectionEditorGripperStyle}"
                            Visibility="{TemplateBinding ResizeGripperVisibility}" />
                    </Grid>
                </Border>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="CollectionEditorStyle" TargetType="propertyGrid:CollectionEditor">
        <Setter Property="Template" Value="{StaticResource CollectionEditor_Template}" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="{telerik1:MaterialResource ResourceKey=MainBrush}" />
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerSolidBrush}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Padding" Value="24,0,16,0" />
        <Setter Property="Width" Value="580" />
        <Setter Property="Height" Value="404" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
    </Style>
    <Style BasedOn="{StaticResource CollectionEditorStyle}" TargetType="propertyGrid:CollectionEditor" />
    <ControlTemplate x:Key="CollectionEditorPicker_Template" TargetType="propertyGrid:CollectionEditorPicker">
        <Border
            Background="{TemplateBinding Background}"
            BorderBrush="{TemplateBinding BorderBrush}"
            BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    x:Name="PART_CollectionEditorTextBlock"
                    Margin="{TemplateBinding Padding}"
                    VerticalAlignment="Center"
                    Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Source}" />
                <telerik:RadDropDownButton
                    x:Name="DropDownButton"
                    Grid.Column="1"
                    MinWidth="36"
                    Margin="0"
                    Padding="0"
                    HorizontalContentAlignment="Left"
                    BorderBrush="{x:Null}"
                    DropDownIndicatorVisibility="Collapsed"
                    IsEnabled="{TemplateBinding IsEnabled}"
                    PopupPlacementTarget="{Binding RelativeSource={RelativeSource TemplatedParent}}">
                    <TextBlock
                        Margin="10"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        telerik:GlyphAdorner.ShowGlyphInDesignTime="True"
                        FontFamily="{StaticResource TelerikWebUI}"
                        FontSize="16"
                        FontStyle="Normal"
                        FontWeight="Normal"
                        Foreground="{telerik1:MaterialResource ResourceKey=IconBrush}"
                        Opacity="{telerik1:MaterialResource ResourceKey=PrimaryOpacity}">
                        <Run x:Name="Arrow" Text="{StaticResource GlyphArrow60Down}" />
                    </TextBlock>
                    <telerik:RadDropDownButton.DropDownContent>
                        <propertyGrid:CollectionEditor
                            x:Name="PART_CollectionEditor"
                            BorderThickness="0"
                            Header="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Header}"
                            IsReadOnly="{TemplateBinding IsReadOnly}"
                            ResizeGripperVisibility="Visible"
                            Source="{Binding RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Path=Source}" />
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger SourceName="DropDownButton" Property="IsOpen" Value="True">
                <Setter TargetName="Arrow" Property="Text" Value="{StaticResource GlyphArrow60Up}" />
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="CollectionEditorPickerStyle" TargetType="propertyGrid:CollectionEditorPicker">
        <Setter Property="Template" Value="{StaticResource CollectionEditorPicker_Template}" />
        <Setter Property="FontFamily" Value="{telerik1:MaterialResource ResourceKey=FontFamily}" />
        <Setter Property="FontSize" Value="{telerik1:MaterialResource ResourceKey=FontSize}" />
        <Setter Property="Foreground" Value="{telerik1:MaterialResource ResourceKey=MarkerBrush}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=DividerBrush}" />
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="Padding" Value="16,0" />
        <Setter Property="BorderThickness" Value="0,0,0,1" />
        <Setter Property="IsTabStop" Value="False" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource StaticValidationTooltipTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource CollectionEditorPickerStyle}" TargetType="propertyGrid:CollectionEditorPicker" />
    <ControlTemplate x:Key="FlagEnumEditorNonEditableTemplate" TargetType="propertyGrid:FlagEnumEditor">
        <telerik:RadDropDownButton
            x:Name="PART_EnumValuesDropDownButton"
            Padding="0"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Center"
            ClickMode="Press"
            Focusable="False"
            IsTabStop="False"
            TabIndex="1">
            <telerik:RadDropDownButton.DropDownContent>
                <ScrollViewer
                    Padding="0"
                    BorderThickness="0"
                    ScrollViewer.HorizontalScrollBarVisibility="Auto"
                    ScrollViewer.VerticalScrollBarVisibility="Auto">
                    <ItemsControl BorderThickness="0" ItemsSource="{Binding EnumItemsSource, RelativeSource={RelativeSource TemplatedParent}}">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <CheckBox
                                    Margin="0"
                                    VerticalContentAlignment="Center"
                                    Content="{Binding Value}"
                                    IsChecked="{Binding IsSelected, Mode=TwoWay}" />
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </ScrollViewer>
            </telerik:RadDropDownButton.DropDownContent>
            <TextBlock
                x:Name="FlagEnumEditor_InputTextBlock"
                Margin="{TemplateBinding Padding}"
                Text="{Binding Value, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, ValidatesOnExceptions=True, ValidatesOnDataErrors=True}" />
        </telerik:RadDropDownButton>
    </ControlTemplate>
    <Style x:Key="FlagEnumEditorStyle" TargetType="propertyGrid:FlagEnumEditor">
        <Setter Property="Template" Value="{StaticResource FlagEnumEditorNonEditableTemplate}" />
        <Setter Property="MinHeight" Value="36" />
        <Setter Property="Padding" Value="16,0" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource StaticValidationTooltipTemplate}" />
    </Style>
    <Style BasedOn="{StaticResource FlagEnumEditorStyle}" TargetType="propertyGrid:FlagEnumEditor" />
</ResourceDictionary>
