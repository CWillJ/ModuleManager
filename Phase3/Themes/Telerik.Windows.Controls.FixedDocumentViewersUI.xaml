                        <mat:Shadow Grid.Row="1" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                        <Border Opacity="0.75" Grid.Row="1" BorderBrush="{telerik1:MaterialResource ResourceKey=DividerBrush}" Background="{telerik1:MaterialResource ResourceKey=MainBrush}"/>
                        <Grid Visibility="{TemplateBinding DragTooltipVisibility}" Grid.Row="1" MinHeight="36">
