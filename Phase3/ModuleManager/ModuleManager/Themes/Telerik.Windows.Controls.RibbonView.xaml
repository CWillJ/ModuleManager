                    </Border>
                </Grid>
            </Popup>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsOpen" Value="True">
                <Setter TargetName="DropDownIndicatorRun" Property="Text" Value="{StaticResource GlyphArrow60Left}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Image" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="textRow1" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
                <Setter TargetName="DropDownIndicator" Property="Opacity" Value="{telerik1:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="DropDownPlacement" Value="Relative">
                <Setter TargetName="DropDownPopup" Property="Placement" Value="Relative"/>
            </Trigger>
            <Trigger Property="FlowDirection" Value="RightToLeft">
                <Setter TargetName="DropDownIndicator" Property="Text" Value="{StaticResource GlyphArrow60Left}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
