                        <Trigger Property="IsKeyboardFocused" Value="True" SourceName="NameBoxInputBox">
                            <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
                            <Setter Property="Visibility" TargetName="Line" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="IsOpen" Value="True" SourceName="DropDownButton">
                            <Setter Property="Background" Value="{telerik:MaterialResource ResourceKey=MainBrush}"/>
                            <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth2"/>
