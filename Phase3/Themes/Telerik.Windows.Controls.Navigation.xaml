                            Margin="16"
                            VerticalAlignment="Center"
                            ContentTemplate="{TemplateBinding IconTemplate}" />
                        <StackPanel
                            x:Name="PromptContent"
                            Grid.Row="0"
                            Grid.Column="1"
                            Margin="0,16">
                            <ScrollViewer
                                Grid.Row="0"
                                Grid.Column="1"
                                BorderBrush="{x:Null}"
