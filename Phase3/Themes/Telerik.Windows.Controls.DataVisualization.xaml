                            <Setter TargetName="label" Property="Opacity" Value="1"/>
                            <Setter TargetName="border" Property="Background" Value="{telerik1:MaterialResource ResourceKey=AccentNormalBrush}"/>
                            <Setter TargetName="border" Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=AccentNormalBrush}"/>
