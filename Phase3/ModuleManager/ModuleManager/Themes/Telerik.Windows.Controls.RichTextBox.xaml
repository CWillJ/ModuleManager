        <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" TargetName="MinimizeButtonContent" Value="{telerikControls:MaterialResource ResourceKey=DisabledOpacity}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="MinimizeButtonGlyph" Property="Text" Value="{StaticResource GlyphUnpin}"/>
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewExpandRibbon}"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="False">
                <Setter TargetName="RootElement" Property="telerikRibbonView:ScreenTip.Title" Value="{telerik:LocalizableResource Key=RibbonViewMinimizeRibbon}"/>
            </Trigger>
        </ControlTemplate.Triggers>
