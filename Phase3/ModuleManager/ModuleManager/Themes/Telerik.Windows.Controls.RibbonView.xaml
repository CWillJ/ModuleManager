                                <mat:Shadow ShadowDepth="Depth2" Background="{telerik1:MaterialResource ResourceKey=MainBrush}" CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"/>
                                <Border x:Name="DropDownPopupBorder"
                                        CornerRadius="{telerik1:MaterialResource ResourceKey=CornerRadiusBottom}"
