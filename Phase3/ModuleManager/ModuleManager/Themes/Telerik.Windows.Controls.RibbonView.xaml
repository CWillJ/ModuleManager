                <Setter Property="VerticalContentAlignment" Value="Center"/>
                <Setter TargetName="ContentGrid" Property="Margin" Value="12 0"/>
                <Setter TargetName="Image" Property="Margin" Value="0"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
