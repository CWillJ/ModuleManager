    <Style
        x:Key="LayoutControlStructureTabTreeViewItem"
        BasedOn="{StaticResource RadTreeViewItemStyle}"
        TargetType="telerikNavigation:RadTreeViewItem">
        <Setter Property="IsExpanded" Value="True" />
        <Setter Property="Background" Value="#01FFFFFF" />
        <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        <Setter Property="Padding" Value="0" />
    </Style>
    <HierarchicalDataTemplate x:Key="StructureItemHierarchicalDataTemplate" ItemsSource="{Binding Items}">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Path
                Margin="10"
                VerticalAlignment="Center"
                Data="{Binding OriginalItemType, Converter={StaticResource TypeToPathDataConverter}}"
                Fill="{Binding Foreground, ElementName=HeaderText}"
                Opacity="{telerik1:MaterialResource ResourceKey=SecondaryOpacity}" />
            <TextBlock
                x:Name="HeaderText"
                Grid.Column="1"
                Margin="12,0,0,0"
                VerticalAlignment="Center"
                Opacity="{telerik1:MaterialResource ResourceKey=PrimaryOpacity}"
                Text="{Binding Header}" />
        </Grid>
    </HierarchicalDataTemplate>
    <Style x:Key="LayoutControlToolBoxViewStyle" TargetType="layoutControl:LayoutControlToolBoxView">
        <Setter Property="telerik:DragDropManager.AllowDrag" Value="True" />
        <Setter Property="mat:MaterialAssist.ShadowDepth" Value="Depth1" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlToolBoxView">
                    <Grid>
                        <telerikNavigation:LayoutControlTabGroup
                            Grid.Row="1"
                            Padding="1"
                            Background="{TemplateBinding Background}">
                            <telerikNavigation:LayoutControlTabGroupItem Header="Items">
                                <StackPanel Orientation="Vertical">
                                    <Grid Margin="3">
                                        <mat:Shadow Background="{telerik1:MaterialResource ResourceKey=MainBrush}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                                        <telerik:RadExpander
                                            Padding="0"
                                            Header="New Items"
                                            HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}"
                                            IsExpanded="True">
                                            <telerik:RadListBox
                                                mat:MaterialAssist.ShadowDepth="Depth0"
                                                BorderThickness="0"
                                                ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                ItemsSource="{Binding NewItems, RelativeSource={RelativeSource TemplatedParent}}"
                                                ScrollViewer.VerticalScrollBarVisibility="Disabled" />
                                        </telerik:RadExpander>
                                    </Grid>
                                    <Grid Margin="3">
                                        <mat:Shadow Background="{telerik1:MaterialResource ResourceKey=MainBrush}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                                        <telerik:RadExpander
                                            Padding="0"
                                            Header="Deleted Items"
                                            HeaderButtonStyle="{StaticResource LayoutControlToolBoxExpanderHeaderButton}"
                                            IsExpanded="True"
                                            Visibility="{TemplateBinding TrackDeletedItems,
                                                                         Converter={StaticResource BooleanToVisibilityConverter}}">
                                            <telerik:RadListBox
                                                x:Name="deletedItemsList"
                                                mat:MaterialAssist.ShadowDepth="Depth0"
                                                BorderThickness="0"
                                                ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}"
                                                ItemsSource="{Binding DeletedItems, RelativeSource={RelativeSource TemplatedParent}}"
                                                ScrollViewer.VerticalScrollBarVisibility="Disabled"
                                                Visibility="{Binding HasItems, RelativeSource={RelativeSource Self}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                                            <telerik:RadExpander.Style>
                                                <Style BasedOn="{StaticResource RadExpanderStyle}" TargetType="telerik:RadExpander">
                                                    <Style.Triggers>
                                                        <DataTrigger Binding="{Binding Path=Items.Count, ElementName=deletedItemsList}" Value="0">
                                                            <Setter Property="IsEnabled" Value="False" />
                                                        </DataTrigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </telerik:RadExpander.Style>
                                        </telerik:RadExpander>
                                    </Grid>
                                </StackPanel>
                            </telerikNavigation:LayoutControlTabGroupItem>
                            <telerikNavigation:LayoutControlTabGroupItem Header="Structure">
                                <telerikNavigation:RadTreeView
                                    x:Name="PART_StructureTree"
                                    ItemContainerStyle="{StaticResource LayoutControlStructureTabTreeViewItem}"
                                    ItemTemplate="{StaticResource StructureItemHierarchicalDataTemplate}" />
                            </telerikNavigation:LayoutControlTabGroupItem>
                        </telerikNavigation:LayoutControlTabGroup>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlResizerStyle" TargetType="{x:Type Thumb}">
        <Setter Property="Background" Value="#01FFFFFF" />
        <Setter Property="MinHeight" Value="11" />
        <Setter Property="MinWidth" Value="11" />
        <Setter Property="SnapsToDevicePixels" Value="True" />
