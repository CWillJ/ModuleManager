                    CanContentScroll="True"
                    FocusVisualStyle="{x:Null}"
                    HorizontalScrollBarVisibility="Auto"
                    VerticalScrollBarVisibility="Hidden">
                    <ItemsPresenter />
                    <ScrollViewer.InputBindings>
                        <KeyBinding
                            Key="A"
