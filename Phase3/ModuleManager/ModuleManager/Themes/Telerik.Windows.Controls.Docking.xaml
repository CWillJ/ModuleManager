                <Setter TargetName="HeaderCloseButton" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="HeaderDropDownMenu" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=IconBrush}"/>
                <Setter TargetName="TitleElement" Property="Foreground" Value="{telerik:MaterialResource ResourceKey=MarkerBrush}"/>
                <Setter TargetName="HeaderPinButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderPinButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderUnPinButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderUnPinButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderCloseButton" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
                <Setter TargetName="HeaderCloseButton" Property="mat:MaterialAssist.PressedBrush" Value="{telerik:MaterialResource ResourceKey=AccentPressedBrush}"/>
                <Setter TargetName="HeaderDropDownMenu" Property="mat:MaterialAssist.MouseOverBrush" Value="{telerik:MaterialResource ResourceKey=AccentHoverBrush}"/>
