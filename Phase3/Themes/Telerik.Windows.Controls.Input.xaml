                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="BorderVisual" Property="Opacity" Value="1" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}" />
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="BorderVisual" Property="Opacity" Value="1" />
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:MaterialResource ResourceKey=PrimaryFocusBrush}" />
