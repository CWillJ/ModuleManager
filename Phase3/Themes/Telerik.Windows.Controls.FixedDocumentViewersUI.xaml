            <Setter.Value>
                <DataTemplate>
                    <Grid Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsDropPossible, Converter={StaticResource InvertedBooleanToVisibilityConverter}}" Margin="10 8">
                        <TextBlock
                                FontFamily="{StaticResource TelerikWebUI}"
