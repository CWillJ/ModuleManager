                                    </TextBlock>
                                </Grid>
                            </Grid>
                        </Border>
                        <Thumb
                            x:Name="PART_TopResizer"
                            Margin="0,-5,0,0"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Top"
                            Cursor="SizeNS"
                            Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb
                            x:Name="PART_LeftResizer"
                            Margin="-5,0,0,0"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Stretch"
                            Cursor="SizeWE"
                            Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb
                            x:Name="PART_RightResizer"
                            Margin="0,0,-5,0"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Stretch"
                            Cursor="SizeWE"
                            Style="{StaticResource LayoutControlResizerStyle}" />
                        <Thumb
                            x:Name="PART_BottomResizer"
                            Margin="0,0,0,-5"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Bottom"
                            Cursor="SizeNS"
                            Style="{StaticResource LayoutControlResizerStyle}" />
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsContextItemLocked" Value="True">
                            <Setter TargetName="LockPathGrid" Property="Opacity" Value="1" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlMouseOverIndicatorStyle" TargetType="layoutControl:LayoutControlMouseOverIndicator">
