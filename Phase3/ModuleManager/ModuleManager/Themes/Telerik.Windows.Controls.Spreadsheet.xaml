                                            </DataTemplate>
                                        </grid:GridViewColumn.CellTemplate>
                                    </grid:GridViewColumn>
                                    <grid:GridViewColumn MinWidth="75">
                                        <grid:GridViewColumn.Header>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_Value"/>
                                        </grid:GridViewColumn.Header>
                                        <grid:GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                        Content="{Binding Path=ResultValue}"
                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=grid:RadGridView} , Path=DataContext.GoToCellCommand}"
                                                        CommandParameter="{Binding Path=FoundCell}"
                                                        Style="{StaticResource LinkButtonStyle}"/>
                                            </DataTemplate>
                                        </grid:GridViewColumn.CellTemplate>
                                    </grid:GridViewColumn>
                                    <grid:GridViewColumn MinWidth="75">
                                        <grid:GridViewColumn.Header>
                                            <TextBlock telerik:LocalizationManager.ResourceKey="Spreadsheet_FindAndReplaceDialog_Formula"/>
                                        </grid:GridViewColumn.Header>
                                        <grid:GridViewColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button
                                                        Content="{Binding Path=RawValue}"
                                                        Command="{Binding RelativeSource={RelativeSource AncestorType=grid:RadGridView} , Path=DataContext.GoToCellCommand}"
                                                        CommandParameter="{Binding Path=FoundCell}"
                                                        Style="{StaticResource LinkButtonStyle}"/>
                                            </DataTemplate>
                                        </grid:GridViewColumn.CellTemplate>
                                    </grid:GridViewColumn>
                                </grid:RadGridView.Columns>
                            </grid:RadGridView>
                        </StackPanel>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
