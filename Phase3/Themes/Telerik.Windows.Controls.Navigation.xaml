            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadWindow">
                    <Grid Margin="3">
                        <mat:Shadow Background="{TemplateBinding Background}" ShadowDepth="{TemplateBinding mat:MaterialAssist.ShadowDepth}" />
                        <Border
                            x:Name="WindowOuterBorder"
                            Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                            <AdornerDecorator>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>
                                    <Border
                                        x:Name="Header"
                                        Background="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                        BorderBrush="{telerik1:MaterialResource ResourceKey=PrimaryNormalBrush}"
                                        BorderThickness="0">
                                        <DockPanel Margin="24,0,0,0" HorizontalAlignment="Stretch">
                                            <ContentControl
                                                x:Name="HeaderContent"
                                                HorizontalAlignment="Stretch"
                                                VerticalAlignment="Center"
                                                HorizontalContentAlignment="Stretch"
                                                Content="{TemplateBinding Header}"
                                                FontFamily="{TemplateBinding FontFamily}"
                                                FontSize="{TemplateBinding FontSize}"
                                                Foreground="{TemplateBinding Foreground}"
                                                IsTabStop="False"
                                                SnapsToDevicePixels="True" />
                                            <telerik:RadButton
                                                x:Name="PART_CloseButton"
                                                Padding="16"
                                                HorizontalAlignment="Right"
                                                mat:MaterialAssist.MouseOverBrush="{telerik1:MaterialResource ResourceKey=PrimaryHoverBrush}"
                                                mat:MaterialAssist.PressedBrush="{telerik1:MaterialResource ResourceKey=PrimaryPressedBrush}"
                                                Background="Transparent"
                                                BorderBrush="Transparent"
                                                Command="{x:Static telerik:WindowCommands.Close}"
                                                Content="{StaticResource GlyphClose}"
                                                CornerRadius="0"
                                                DockPanel.Dock="Right"
                                                FontFamily="{StaticResource TelerikWebUI}"
                                                FontSize="16"
                                                FontStyle="Normal"
                                                FontWeight="Normal"
                                                Foreground="{telerik1:MaterialResource ResourceKey=MarkerInvertedBrush}" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="ContentOuterBorder"
                                        Grid.Row="1"
                                        Background="{x:Null}"
                                        BorderBrush="{TemplateBinding BorderBrush}">
                                        <ContentPresenter
                                            x:Name="ContentElement"
                                            Margin="{TemplateBinding Padding}"
                                            Content="{TemplateBinding Content}"
                                            ContentTemplate="{TemplateBinding ContentTemplate}" />
                                    </Border>
                                </Grid>
                            </AdornerDecorator>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="LayoutControlToolBoxStyle" TargetType="layoutControl:LayoutControlToolBox">
        <Setter Property="Canvas.Right" Value="0" />
        <Setter Property="Panel.ZIndex" Value="100" />
        <Setter Property="Margin" Value="5" />
        <Setter Property="Padding" Value="10" />
        <Setter Property="WindowStyle" Value="{StaticResource LayoutControlToolBoxWindowStyle}" />
